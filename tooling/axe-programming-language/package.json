{
  "name": "axe-programming-language",
  "displayName": "Axe Programming Language",
  "description": "Language support for Axe",
  "publisher": "NavidM",
  "icon": "icon.png",
  "author": {
    "name": "Navid M"
  },
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/axelang/axe.git"
  },
  "engines": {
    "vscode": ">=1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "axe",
        "aliases": [
          "Axe",
          "axe"
        ],
        "extensions": [
          ".axe",
          ".axec"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "axe",
        "scopeName": "source.axe",
        "path": "./syntaxes/axe.tmLanguage.json"
      }
    ]
  }
}