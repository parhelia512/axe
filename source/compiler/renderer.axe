                        newname = StringBuilder.to_string(addr(sb_name));
                        val constraint_str: string = StringBuilder.to_string(addr(sb_const));
                        StringBuilder.destroy(addr(sb_name));
                        StringBuilder.destroy(addr(sb_const));

                        if str_len(constraint_str) > 0 {
                            mut existing: string = StringStringMap.get(addr(g_needed_specializations), emitted_name);
                            mut added: bool = false;
                            if str_len(existing) > 0 {
                                if find_substr(existing, constraint_str) < 0 {
                                    existing = concat(existing, str(";"));
                                    existing = concat(existing, constraint_str);
                                    StringStringMap.set(addr(g_needed_specializations), addr(g_renderer_arena), emitted_name, existing);
                                    added = true;
                                }
                            } else {
                                StringStringMap.set(addr(g_needed_specializations), addr(g_renderer_arena), emitted_name, constraint_str);
                                added = true;
                            }

                            if added {
                                StringList.push(g_specialization_queue, addr(g_renderer_arena), emitted_name);

                                if !StringBoolMap.contains(addr(g_emitted_prototypes), newname) {
                                    mut spec_arena_p: Arena = Arena.create(1024);
                                    mut type_mapping_p: StringStringMap = deref(StringStringMap.create(addr(spec_arena_p), 8));
                                    mut tm_idx_p: i32 = 0;
                                    mut tm_start_p: i32 = 0;
                                    val tm_len_p: i32 = cast[i32](str_len(constraint_str));
                                    loop {
                                        if tm_idx_p > tm_len_p { break; }
                                        mut is_tm_sep_p: bool = false;
                                        if tm_idx_p == tm_len_p { is_tm_sep_p = true; }
                                        elif get_char(constraint_str, tm_idx_p) == ',' { is_tm_sep_p = true; }
                                        if is_tm_sep_p {
                                            val tm_constraint_p: string = substring_se(constraint_str, tm_start_p, tm_idx_p);
                                            val tm_colon_p: i32 = find_char_from(tm_constraint_p, ':', cast[usize](0));
                                            if tm_colon_p > 0 {
                                                val tm_param_p: string = substring_se(tm_constraint_p, 0, tm_colon_p);
                                                val tm_concrete_p: string = substring_se(tm_constraint_p, tm_colon_p + 1, cast[i32](str_len(tm_constraint_p)));
                                                StringStringMap.set(addr(type_mapping_p), addr(spec_arena_p), tm_param_p, tm_concrete_p);
                                            }
                                            tm_start_p = tm_idx_p + 1;
                                        }
                                        tm_idx_p++;
                                    }
                                    val proto: string = generate_specialized_function_prototype(gen_ast, addr(type_mapping_p), newname);
                                    if str_len(proto) > 0 {
                                        StringBuilder.append(addr(g_specialization_prototypes_sb), proto);
                                        StringBoolMap.set(addr(g_emitted_prototypes), addr(g_renderer_arena), newname, true);
                                    }
                                }
                            }
                        }
                    } else {
                        if str_len(inferred2) > 0 and str_len(inferred1) > 0 {
                            newname = concat(newname, str("__T2_"));
                            newname = concat(newname, sanitize_c_identifier(map_axe_type_to_c(inferred2)));
                            newname = concat(newname, str("__T_"));
                            newname = concat(newname, sanitize_c_identifier(map_axe_type_to_c(inferred1)));
                        } elif str_len(inferred1) > 0 {
                            newname = concat(newname, str("__T_"));
                            newname = concat(newname, sanitize_c_identifier(map_axe_type_to_c(inferred1)));
                        } elif expected_type_used and str_len(expected_type_for_inference) > 0 {
                            newname = concat(newname, str("__T_"));
                            newname = concat(newname, sanitize_c_identifier(map_axe_type_to_c(expected_type_for_inference)));
                        }
                    }
                    emitted_name = newname;
                }
            }
        }

        emitted_name = apply_extern_aliases(emitted_name);

        mut sb_call: StringBuilder = StringBuilder.init(512);
        StringBuilder.append(addr(sb_call), emitted_name);
        StringBuilder.append_char(addr(sb_call), '(');

        val args: ref list(string) = ast.data.func_call.args;
        if args != nil {
            mut arg_idx: i32 = 0;
            loop {
                if arg_idx >= len(deref(args)) {
                    break;
                }

                if arg_idx > 0 {
                    StringBuilder.append_c(addr(sb_call), ", ");
                }

                val raw_arg: string = args.data[arg_idx];
                val processed_arg: string = process_expression(raw_arg);
                val rewritten_arg: string = rewrite_generic_calls(processed_arg);
                StringBuilder.append(addr(sb_call), rewritten_arg);
                arg_idx++;
            }
        }

        StringBuilder.append_c(addr(sb_call), ");\n");
        mut final_call: string = StringBuilder.to_string(addr(sb_call));
        StringBuilder.destroy(addr(sb_call));
        final_call = rewrite_generic_calls(final_call);
        
        return final_call;
    }
    
    if equals_c(node_type, "Print") {
        val messages_ref: ref list(string) = ast.data.print.messages;
        val flags_ref: ref list(bool) = ast.data.print.is_expressions;

        if messages_ref == nil or flags_ref == nil {
            return result;
        }

        val messages: list(string) = deref(messages_ref);
        val flags: list(bool) = deref(flags_ref);

        if len(messages) == 0 {
            return result;
        }

        if len(messages) == 1 and len(flags) == 1 and !flags.data[0] {
            mut sb_print: StringBuilder = StringBuilder.init(256);
            StringBuilder.append_c(addr(sb_print), "printf(\"");
            StringBuilder.append(addr(sb_print), messages.data[0]);
            StringBuilder.append_c(addr(sb_print), "\");\n");
            return StringBuilder.to_string(addr(sb_print));
        }

        mut sb_print: StringBuilder = StringBuilder.init(1024);
        
        if len(messages) == 1 and len(flags) == 1 and !flags.data[0] {
            StringBuilder.append_c(addr(sb_print), "printf(\"");
            StringBuilder.append(addr(sb_print), messages.data[0]);
            StringBuilder.append_c(addr(sb_print), "\");\n");
        } else {
             StringBuilder.append_c(addr(sb_print), "printf(\"");
        }
        
        result = strip_all_c_prefixes(result);
        
        result = rewrite_generic_calls(result);
        
        if str_len(result) == 0 {
             StringBuilder.clear(addr(sb_print));
             StringBuilder.append_c(addr(sb_print), "printf(\"");
             mut i: i32 = 0;
             loop {
                 if i >= len(messages) { break; }
                 if flags.data[i] {
                     StringBuilder.append_c(addr(sb_print), "%s");
                 } else {
                     StringBuilder.append(addr(sb_print), messages.data[i]);
                 }
                 i++;
             }
             StringBuilder.append_c(addr(sb_print), "\"");
             
             i = 0;
             loop {
                 if i >= len(messages) { break; }
                 if flags.data[i] {
                     StringBuilder.append_c(addr(sb_print), ", ");
                     val processed: string = process_expression(messages.data[i]);
                     StringBuilder.append(addr(sb_print), processed);
                 }
                 i++;
             }
             StringBuilder.append_c(addr(sb_print), ");\n");
             result = StringBuilder.to_string(addr(sb_print));
        }
        
        StringBuilder.destroy(addr(sb_print));
        return result;
    }
    
    if equals_c(node_type, "Loop") {
        mut sb_loop: StringBuilder = StringBuilder.init(1024);
        StringBuilder.append_c(addr(sb_loop), "while (1) {\n");
        
        if ast.children != nil {
            val children: ref list(ASTNode) = ast.children;
            mut regular_children: list(ref ASTNode);
            mut defer_children: list(ref ASTNode);

            mut i: i32 = 0;
            loop {
                if i >= len(deref(children)) {
                    break;
                }
                val child: ref ASTNode = addr(children.data[i]);
                if equals_c(child.node_type, "Defer") {
                    append(defer_children, child);
                } else {
                    append(regular_children, child);
                }
                i++;
            }

            mut reg_idx: i32 = 0;
            loop {
                if reg_idx >= len(regular_children) {
                    break;
                }
                val child: ref ASTNode = regular_children.data[reg_idx];
                val child_code: string = generate_c(child);
                StringBuilder.append(addr(sb_loop), child_code);
                reg_idx++;
            }

            mut defer_idx: i32 = 0;
            loop {
                if defer_idx >= len(defer_children) {
                    break;
                }
                val defer_node: ref ASTNode = defer_children.data[defer_idx];
                val body: ref list(ASTNode) = defer_node.data.defer_node.body;
                if body != nil {
                    mut i_defer: i32 = 0;
                    loop {
                        if i_defer >= len(deref(body)) {
                            break;
                        }
                        val defer_child_code: string = generate_c(addr(body.data[i_defer]));
                        StringBuilder.append(addr(sb_loop), defer_child_code);
                        i_defer++;
                    }
                }
                defer_idx++;
            }
        }
        StringBuilder.append_c(addr(sb_loop), "}\n");
        return StringBuilder.to_string(addr(sb_loop));
    }
    
    if equals_c(node_type, "WhenBranch") {
        // Check if all type constraints match the current type mapping context
        if g_current_type_mapping != nil {
            val tp_list: ref list(string) = ast.data.when_branch.type_params;
            val ct_list: ref list(string) = ast.data.when_branch.concrete_types;
            
            if tp_list != nil and ct_list != nil {
                mut all_match: bool = true;
                mut ci: i32 = 0;
                loop {
                    if ci >= len(deref(tp_list)) { break; }
                    val tp: string = tp_list.data[ci];
                    val ct_expected_axe: string = ct_list.data[ci];
                    val ct_expected: string = map_axe_type_to_c(ct_expected_axe);

                    if StringStringMap.contains(g_current_type_mapping, tp) {
                        val ct_actual: string = StringStringMap.get(g_current_type_mapping, tp);
                        
                        if ::loud_logger {
                            print "Match attempt: "; print tp; print " -> "; print ct_actual;
                            print " vs expected "; println ct_expected;
                        }

                        if !types_match(ct_actual, ct_expected) {
                            all_match = false;
                            break;
                        }
                    } else {
                        // Type param not in mapping - doesn't match
                        all_match = false;
                        break;
                    }
                    ci++;
                }
                
                if !all_match {
                    // Constraints don't match - don't emit any code
                    return str("");
                }
            }
        }
        
        // Constraints match (or no context) - emit children
        mut sb_when: StringBuilder = StringBuilder.init(1024);
        
        if ast.children != nil {
            val wchildren: ref list(ASTNode) = ast.children;
            mut wi: i32 = 0;
            loop {
                if wi >= len(deref(wchildren)) {
                    break;
                }
                val wchild_code: string = generate_c(addr(wchildren.data[wi]));
                StringBuilder.append(addr(sb_when), wchild_code);
                wi++;
            }
        }
        
        return StringBuilder.to_string(addr(sb_when));
    }

    if equals_c(node_type, "Platform") {
        val platform_name: string = ast.data.platform_node.platform_name;

        mut sb_plat: StringBuilder = StringBuilder.init(1024);
        val plat_dir: string = platform_macro_start(platform_name);
        if str_len(plat_dir) > 0 {
            StringBuilder.append(addr(sb_plat), plat_dir);
        }

        if ast.children != nil {
            val children: ref list(ASTNode) = ast.children;
            mut i: i32 = 0;
            loop {
                if i >= len(deref(children)) {
