                        } elif ch == ')' {
                            depth--;
                            if depth == 0 {
                                break;
                            }
                        }
                        paren_pos++;
                    }
                    if depth == 0 {
                        val element_type: string = strip(substring_se(actual_var_type, 5, paren_pos));
                        mut resolved_element_type: string = element_type;
                        if g_current_type_mapping != nil and StringStringMap.contains(g_current_type_mapping, element_type) {
                            resolved_element_type = StringStringMap.get(g_current_type_mapping, element_type);
                        }
                        val c_element_type: string = map_axe_type_to_c(resolved_element_type);
                        val sanitized_element: string = sanitize_list_element_name(c_element_type);

                        if !StringBoolMap.contains(addr(g_list_element_types), c_element_type) {
                            StringBoolMap.set(addr(g_list_element_types), addr(g_renderer_arena), c_element_type, true);
                            StringList.push(g_list_elements, addr(g_renderer_arena), c_element_type);
                        }

                        val processed_value: string = process_expression(value);

                        mut sb_append_inferred: StringBuilder = StringBuilder.init(256);
                        StringBuilder.append_c(addr(sb_append_inferred), "__list_");
                        StringBuilder.append(addr(sb_append_inferred), sanitized_element);
                        StringBuilder.append_c(addr(sb_append_inferred), "_push(&");
                        StringBuilder.append(addr(sb_append_inferred), var_name);
                        StringBuilder.append_c(addr(sb_append_inferred), ", ");
                        StringBuilder.append(addr(sb_append_inferred), processed_value);
                        StringBuilder.append_c(addr(sb_append_inferred), ");\n");

                        debug_print_raw "\n[DEBUG] Generated append code (resolved/inferred) for '";
                        debug_print_str var_name;
                        debug_print_raw "'";
                        return StringBuilder.to_string(addr(sb_append_inferred));
                    }
                }
            }
        }

        mut emitted_name: string = func_name;
        if StringStringMap.contains(addr(g_function_prefixes), func_name) {
            emitted_name = StringStringMap.get(addr(g_function_prefixes), func_name);
        }

        if call_args != nil and len(deref(call_args)) > 0 {
            mut is_generic_em: bool = StringBoolMap.contains(addr(g_generic_functions), emitted_name);
            if !is_generic_em {
                if StringBoolMap.contains(addr(g_generic_functions), func_name) {
                    is_generic_em = true;
                } elif StringStringMap.contains(addr(g_function_prefixes), func_name) {
                    val pref_fn: string = StringStringMap.get(addr(g_function_prefixes), func_name);
                    if StringBoolMap.contains(addr(g_generic_functions), pref_fn) {
                        is_generic_em = true;
                    }
                }
            }

            if is_generic_em and find_substr(emitted_name, str("__T_")) < 0 and find_substr(emitted_name, str("__T2_")) < 0 {
                mut inferred1: string = str("");
                mut inferred2: string = str("");
                if len(deref(call_args)) > 0 {
                    inferred1 = infer_expression_type(call_args.data[0]);
                }
                if len(deref(call_args)) > 1 {
                    inferred2 = infer_expression_type(call_args.data[1]);
                }

                mut expected_type_used: bool = false;
                mut expected_type_for_inference: string = str("");

                if str_len(g_expected_return_type) > 0 {
                    if has_prefix(g_expected_return_type, str("list(")) {
                        mut paren_pos: i32 = 5;  // length of "list("
                        mut depth: i32 = 1;
                        loop {
                            if paren_pos >= str_len(g_expected_return_type) {
                                break;
                            }
                            val ch: char = get_char(g_expected_return_type, paren_pos);
                            if ch == '(' {
                                depth++;
                            } elif ch == ')' {
                                depth--;
                                if depth == 0 {
                                    break;
                                }
                            }
                            paren_pos++;
                        }
                        if depth == 0 {
                            expected_type_for_inference = strip(substring_se(g_expected_return_type, 5, paren_pos));
                            expected_type_used = true;
                        }
                    } else {
                        expected_type_for_inference = g_expected_return_type;
                        expected_type_used = true;
                    }
                }

                if str_len(inferred1) > 0 or str_len(inferred2) > 0 or expected_type_used {
                    mut newname: string = emitted_name;
                    if StringLongMap.contains(addr(g_generic_function_asts), emitted_name) {
                        val gen_ast_ptr: i64 = StringLongMap.get(addr(g_generic_function_asts), emitted_name);
                        val gen_ast: ref ASTNode = cast[ref ASTNode](gen_ast_ptr);
                        val type_params: ref list(string) = gen_ast.data.function.type_params;
                        mut sb_name: StringBuilder = StringBuilder.init(128);

                        StringBuilder.append(addr(sb_name), emitted_name);
                        
                        mut sb_const: StringBuilder = StringBuilder.init(128);

                        if type_params != nil {
                            mut arg_based_inference: bool = false;
                            if len(deref(type_params)) > 0 and str_len(inferred1) > 0 {
                                val tp1: string = type_params.data[0];
                                val mapped1: string = map_axe_type_to_c(inferred1);
                                StringBuilder.append_c(addr(sb_name), "__");
                                StringBuilder.append(addr(sb_name), tp1);
                                StringBuilder.append_c(addr(sb_name), "_");
                                StringBuilder.append(addr(sb_name), sanitize_c_identifier(mapped1));

                                StringBuilder.append(addr(sb_const), tp1);
                                StringBuilder.append_c(addr(sb_const), ":");
                                StringBuilder.append(addr(sb_const) , mapped1);
                                arg_based_inference = true;
                            }
                            if len(deref(type_params)) > 1 and str_len(inferred2) > 0 {
                                val tp2: string = type_params.data[1];
                                val mapped2: string = map_axe_type_to_c(inferred2);
                                StringBuilder.append_c(addr(sb_name), "__");
                                StringBuilder.append(addr(sb_name), tp2);
                                StringBuilder.append_c(addr(sb_name), "_");
                                StringBuilder.append(addr(sb_name), sanitize_c_identifier(mapped2));

                                StringBuilder.append_c(addr(sb_const), ",");
                                StringBuilder.append(addr(sb_const), tp2);
                                StringBuilder.append_c(addr(sb_const), ":");
                                StringBuilder.append(addr(sb_const), mapped2);
                                arg_based_inference = true;
                            }

                            if !arg_based_inference and expected_type_used and str_len(expected_type_for_inference) > 0 {
                                if len(deref(type_params)) > 0 {
                                    val tp1: string = type_params.data[0];
                                    val mapped_expected: string = map_axe_type_to_c(expected_type_for_inference);
                                    StringBuilder.append_c(addr(sb_name), "__");
                                    StringBuilder.append(addr(sb_name), tp1);
                                    StringBuilder.append_c(addr(sb_name), "_");
                                    StringBuilder.append(addr(sb_name), sanitize_c_identifier(mapped_expected));

                                    StringBuilder.append(addr(sb_const), tp1);
                                    StringBuilder.append_c(addr(sb_const), ":");
                                    StringBuilder.append(addr(sb_const), mapped_expected);
                                }
                            }
