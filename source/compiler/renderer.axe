/// Helper to extract prefix from a mangled name (e.g. "std__errors__error" -> "std__errors")
def get_prefix_from_mangled(name: string): string {
    val n: i32 = cast[i32](str_len(name));
    mut i: i32 = n - 2;
    loop {
        if i <= 0 {
            break;
        }
        if get_char(name, i) == '_' and get_char(name, i + 1) == '_' {
            return substring_se(name, 0, i);
        }
        i--;
    }
    return str("");
}

/// Set the current source file for #line directives
pub def set_source_file(filename: string) {
    g_current_source_file = filename;
}

/// Enable or disable emitting `#line` directives in generated C.
pub def set_emit_line_directives(v: bool) {
    g_emit_line_directives = v;
}

/// Register a function prefix mapping (original -> prefixed)
pub def renderer_add_prefix(original: string, prefixed: string) {
    mut arena: Arena = Arena.create(1024);
    StringStringMap.set(addr(g_function_prefixes), addr(arena), original, prefixed);
}

/// Check if a specific external header was used (e.g., "pcre.h", "curl/curl.h")
pub def has_external_header(header_name: string): bool {
    return StringList.contains(g_global_external_headers, header_name)
        or StringList.contains(g_windows_external_headers, header_name)
        or StringList.contains(g_posix_external_headers, header_name)
        or StringList.contains(g_linux_external_headers, header_name)
        or StringList.contains(g_macos_external_headers, header_name)
        or StringList.contains(g_freebsd_external_headers, header_name)
        or StringList.contains(g_openbsd_external_headers, header_name)
        or StringList.contains(g_netbsd_external_headers, header_name)
        or StringList.contains(g_dragonflybsd_external_headers, header_name);
}

/// Resolve "Self" keyword in a type string to the current model name
def resolve_self(axe_type: string): string {
    if str_len(g_current_model) == 0 {
        return axe_type;
    }
    if find_substr(axe_type, str("Self")) < 0 {
        return axe_type;
    }
    
    val res: string = replace_all(axe_type, str("Self"), g_current_model);
    return res;
}

/// Return the platform-specific starting preprocessor directive for a
/// given platform name. Empty string if none.
def platform_macro_start(platform_name: string): string {
    if equals_c(platform_name, "windows") {
        return str("#ifdef _WIN32\n");
    } elif equals_c(platform_name, "posix") {
        return str("#ifndef _WIN32\n");
    } elif equals_c(platform_name, "linux") {
        return str("#ifdef __linux__\n");
    } elif equals_c(platform_name, "macos") {
        return str("#ifdef __APPLE__\n");
    } elif equals_c(platform_name, "freebsd") {
        return str("#ifdef __FreeBSD__\n");
    } elif equals_c(platform_name, "openbsd") {
        return str("#ifdef __OpenBSD__\n");
    } elif equals_c(platform_name, "netbsd") {
        return str("#ifdef __NetBSD__\n");
    } elif equals_c(platform_name, "dragonflybsd") {
        return str("#ifdef __DragonFly__\n");
    }
    return str("");
}

/// Emit a #line directive for error mapping
[inline]
def emit_line_directive(line: i32): string {
    if !g_emit_line_directives or line <= 0 {
        return str("");
    }
    mut sb: StringBuilder = StringBuilder.init(64);
    StringBuilder.append_c(addr(sb), "#line ");
    StringBuilder.append(addr(sb), i32_to_string(line));
    if g_current_source_file.data != nil and str_len(g_current_source_file) > 0 {
        mut path_to_emit: string = g_current_source_file;
        path_to_emit = replace_all(path_to_emit, str("\\"), str("/"));
        StringBuilder.append_c(addr(sb), " \"");
        StringBuilder.append(addr(sb), path_to_emit);
        StringBuilder.append_c(addr(sb), "\"");
    }
    StringBuilder.append_c(addr(sb), "\n");
    val result: string = StringBuilder.to_string(addr(sb));
    StringBuilder.destroy(addr(sb));
    return result;
}

