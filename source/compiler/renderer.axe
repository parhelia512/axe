/// Get the base name of a type (part after "__")
def get_type_base_name(t: string): string {
    val tlen: i32 = cast[i32](str_len(t));
    mut i: i32 = tlen - 2;
    loop {
        if i <= 0 {
            break;
        }
        if get_char(t, i) == '_' and get_char(t, i + 1) == '_' {
            return substring_se(t, i + 2, tlen);
        }
        i--;
    }
    return t;
}

/// Check if two types are equivalent, handling module prefixes appropriately
def types_match(actual_type: string, expected_type: string): bool {
    if ::loud_logger {
         print "types_match: '"; print actual_type; print "' vs '"; print expected_type; println "'";
    }

    val actual_type_res: string = resolve_self(actual_type);
    val expected_type_res: string = resolve_self(expected_type);

    if compare(actual_type_res, expected_type_res) == 0 {
        return true;
    }

    val actual_base: string = get_type_base_name(actual_type_res);
    val expected_base: string = get_type_base_name(expected_type_res);
    
    if compare(actual_base, expected_base) == 0 {
        if compare(actual_base, str("error")) == 0 {
            return true;
        }
        if str_contains_c(actual_type_res, "__") or str_contains_c(expected_type_res, "__") {
             return true;
        }
    }

    val expected_len: i32 = cast[i32](str_len(expected_type_res));
    val actual_len: i32 = cast[i32](str_len(actual_type_res));

    if actual_len > expected_len and expected_len >= 2 {
        val suffix_start: i32 = actual_len - expected_len;
        val separator_pos: i32 = suffix_start - 2;

        if separator_pos > 0 and suffix_start <= actual_len {
            if get_char(actual_type_res, separator_pos) == '_' and get_char(actual_type_res, separator_pos + 1) == '_' {
                val actual_suffix: string = substring_se(actual_type_res, suffix_start, actual_len);
                if compare(actual_suffix, expected_type_res) == 0 {
                    return true;
                }
            }
        }
    }

    if expected_len > actual_len and actual_len >= 2 and find_char_from(actual_type_res, '_', cast[usize](0)) < 0 {
        val actual_suffix_len: i32 = cast[i32](str_len(actual_type_res));
        val suffix_start2: i32 = expected_len - actual_suffix_len;
        val separator_pos2: i32 = suffix_start2 - 2;

        if suffix_start2 <= expected_len and separator_pos2 > 0 {
            if get_char(expected_type_res, separator_pos2) == '_' and get_char(expected_type_res, separator_pos2 + 1) == '_' {
                val expected_suffix: string = substring_se(expected_type_res, suffix_start2, expected_len);
                if compare(actual_type_res, expected_suffix) == 0 {
                    return true;
                }
            }
        }
    }

    return false;
}

/// Helper to compare comma-separated constraint strings (e.g. "T:type1,U:type2")
def constraints_match(actual_constraints: string, expected_constraints: string): bool {
    if compare(actual_constraints, expected_constraints) == 0 {
        return true;
    }

    mut a_start: i32 = 0;
    val alen: i32 = cast[i32](str_len(actual_constraints));
    mut e_start: i32 = 0;
    val elen: i32 = cast[i32](str_len(expected_constraints));

    loop {
        if a_start >= alen or e_start >= elen {
            break;
        }

        mut a_end: i32 = find_char_from(actual_constraints, ',', cast[usize](a_start));
        if a_end < 0 { a_end = alen; }
        mut e_end: i32 = find_char_from(expected_constraints, ',', cast[usize](e_start));
        if e_end < 0 { e_end = elen; }

        val a_pair: string = substring_se(actual_constraints, a_start, a_end);
        val e_pair: string = substring_se(expected_constraints, e_start, e_end);
        val a_colon: i32 = find_char_from(a_pair, ':', cast[usize](0));
        val e_colon: i32 = find_char_from(e_pair, ':', cast[usize](0));

        if a_colon > 0 and e_colon > 0 {
            val a_param: string = substring_se(a_pair, 0, a_colon);
            val e_param: string = substring_se(e_pair, 0, e_colon);

            if compare(a_param, e_param) == 0 {
                val a_type: string = substring_se(a_pair, a_colon + 1, cast[i32](str_len(a_pair)));
                val e_type: string = substring_se(e_pair, e_colon + 1, cast[i32](str_len(e_pair)));

                if !types_match(a_type, e_type) {
                    return false;
                }
            } else {
                return false;
            }
        } else {
            if compare(a_pair, e_pair) != 0 {
                return false;
            }
        }

        a_start = a_end + 1;
        e_start = e_end + 1;
    }

    return a_start >= alen and e_start >= elen;
}

/// Check if a type is a builtin primitive type
[inline]
def is_builtin_type(type_name: string): bool {
    return equals_c(type_name, "i8") or equals_c(type_name, "u8") or
        equals_c(type_name, "i16") or equals_c(type_name, "u16") or
        equals_c(type_name, "i32") or equals_c(type_name, "u32") or
        equals_c(type_name, "i64") or equals_c(type_name, "u64") or
        equals_c(type_name, "isize") or equals_c(type_name, "usize") or
        equals_c(type_name, "f32") or equals_c(type_name, "f64") or
        equals_c(type_name, "bool") or equals_c(type_name, "char") or
        equals_c(type_name, "void") or equals_c(type_name, "byte") or
        equals_c(type_name, "rchar") or equals_c(type_name, "ptrdiff") or 
        equals_c(type_name, "Self");
}

/// Extract the base type name from a complex type (removing ref, list, mut, etc.)
def extract_base_type(type_name: string): string {
    mut result: string = strip(type_name);
    
    loop {
        mut changed: bool = false;
        
        if has_prefix(result, str("mut ")) {
            result = strip(substr(result, 4, str_len(result) - 4));
            changed = true;
        }
        
        if has_prefix(result, str("ref ")) {
            result = strip(substr(result, 4, str_len(result) - 4));
            changed = true;
        }
        
        if !changed {
            break;
        }
    }
    
    if has_prefix(result, str("list(")) {
        mut paren_pos: i32 = 5;
        mut depth: i32 = 1;
        loop {
            if paren_pos >= str_len(result) {
                break;
            }
            val ch: char = get_char(result, paren_pos);
            if ch == '(' {
                depth++;
            } elif ch == ')' {
                depth--;
                if depth == 0 {
                    break;
                }
            }
            paren_pos++;
        }
        if depth == 0 {
            result = strip(substring_se(result, 5, paren_pos));
            return extract_base_type(result);
        }
    }
    
    val br_idx: i32 = find_char_from(result, '[', cast[usize](0));
    if br_idx > 0 {
        result = strip(substring_se(result, 0, br_idx));
    }
    
    loop {
        if has_suffix(result, str("*")) {
            result = strip(substring_se(result, 0, str_len(result) - 1));
        } else {
            break;
        }
    }
    
    return result;
}

/// Check if a type is a language keyword (not a user-defined type)
[inline]
def is_language_keyword(name: string): bool {
    if equals_c(name, "union") or equals_c(name, "generic") {
        return true;
    }
    if equals_c(name, "untyped") {
        return true;
    }
    return false;
}

/// Check if a string is all uppercase letters or underscores (constant naming convention)
def is_all_upper_or_underscore(name: string): bool {
    if str_len(name) == 0 {
        return false;
    }
    
    mut i: i32 = 0;
    val n: i32 = cast[i32](str_len(name));
    loop {
        if i >= n {
            break;
        }
        val ch: char = get_char(name, i);
        if !((ch >= 'A' and ch <= 'Z') or (ch >= '0' and ch <= '9') or ch == '_') {
            return false;
        }
        i++;
    }
    return true;
}

/// Check if a name is a C standard constant
[inline]
def is_c_constant(name: string): bool {
    return equals_c(name, "NULL") or equals_c(name, "TRUE") or equals_c(name, "FALSE")
        or equals_c(name, "EOF") or equals_c(name, "SEEK_SET") or equals_c(name, "SEEK_CUR") or equals_c(name, "SEEK_END")
        or has_prefix(name, str("STDIN")) or has_prefix(name, str("STDOUT")) or has_prefix(name, str("STDERR"))
        or has_prefix(name, str("INT_")) or has_prefix(name, str("UINT_")) or has_prefix(name, str("SIZE_"))
        or has_prefix(name, str("PATH_")) or has_prefix(name, str("NAME_"))
        or has_prefix(name, str("S_IS")) or has_prefix(name, str("_S_IF")) or has_prefix(name, str("_A_"))
        or has_prefix(name, str("WEXITSTATUS")) or equals_c(name, "RAND_MAX");
}

/// Check if a function name is a built-in function or C stdlib function
[inline]
def is_builtin_function(name: string): bool {
    return equals_c(name, "append") or equals_c(name, "len")
        or equals_c(name, "assert") or equals_c(name, "cast")
        or equals_c(name, "deref") or equals_c(name, "addr")
        or equals_c(name, "sizeof") or equals_c(name, "malloc") or equals_c(name, "free")
        or equals_c(name, "memcpy") or equals_c(name, "memset") or equals_c(name, "memmove")
        or equals_c(name, "strlen") or equals_c(name, "strcmp") or equals_c(name, "strcpy")
        or equals_c(name, "printf") or equals_c(name, "sprintf") or equals_c(name, "fprintf")
        or equals_c(name, "fopen") or equals_c(name, "fclose") or equals_c(name, "fread") or equals_c(name, "fwrite")
        or equals_c(name, "exit") or equals_c(name, "abort") or equals_c(name, "atexit")
        or (name.len >= 2 and name.data[0] == 'C' and (name.data[1] == '.' or (name.data[1] == ' ' and find_char_from(name, '.', cast[usize](1)) > 0)));
}

/// Check if a symbol is valid (either builtin, imported, or a known model/enum)
def is_valid_symbol(symbol_name: string): bool {
    return str_len(symbol_name) == 0
        or is_builtin_type(symbol_name)
        or is_language_keyword(symbol_name)
        or has_prefix(strip(symbol_name), str("list("))
        or StringBoolMap.contains(addr(g_opaque_types), symbol_name)
        or StringBoolMap.contains(addr(g_foreign_types), symbol_name)
        or is_symbol_imported(symbol_name)
        or StringStringMap.contains(addr(g_model_names), symbol_name)
        or StringBoolMap.contains(addr(g_enum_names), symbol_name)
        or has_prefix(symbol_name, str("struct "))
        or StringBoolMap.contains(addr(g_current_function_type_params), symbol_name);
}

/// Validate that a type is properly imported (call this for user-facing types)
def validate_type_imported(type_name: string) {
    if str_len(type_name) == 0 {
        return;
    }

    if has_prefix(strip(type_name), str("list(")) {
        mut paren_pos: i32 = 5;  // length of "list("
        mut depth: i32 = 1;
        loop {
            if paren_pos >= str_len(type_name) {
                break;
            }
            val ch: char = get_char(type_name, paren_pos);
            if ch == '(' {
                depth++;
            } elif ch == ')' {
                depth--;
                if depth == 0 {
                    break;
                }
            }
            paren_pos++;
        }
        if depth == 0 {
            return;
        }
    }

    val base_type: string = extract_base_type(type_name);

    if str_len(base_type) == 0 {
        return;
    }

    if is_builtin_type(base_type) {
        return;
    }

    if find_char_from(base_type, '_', cast[usize](0)) >= 0 {
        return;
    }

    if !is_valid_symbol(base_type) {
        if g_current_source_file.data != nil {
            print(get_short_filename(g_current_source_file));
        } else {
            print "(null)";
        }
        print ":";
        print(i32_to_string(g_current_line));
        print ": error: undefined symbol: ";
        print base_type;
        println ", perhaps missing 'use' statement?";
        unsafe {
            C.exit(1);
        }
    }
}

/// Register a function's parameter types for later validation
def register_function_signature(func_name: string, params: ref list(string), ret_type: string, arena: ref Arena) {
    val old_model: string = g_current_model;
    
    if str_len(g_current_model) == 0 {
        mut dot_pos: i32 = find_char_from(func_name, '.', cast[usize](0));
        if dot_pos > 0 {
            g_current_model = substring_se(func_name, 0, dot_pos);
        } else {
            mut last_sep: i32 = -1;
            mut si: i32 = 0;
            val slen: i32 = cast[i32](str_len(func_name));
            loop {
                if si + 1 >= slen { break; }
                if get_char(func_name, si) == '_' and get_char(func_name, si+1) == '_' {
                    last_sep = si;
                }
                si++;
            }
            if last_sep >= 0 {
                val model_candidate: string = substring_se(func_name, 0, last_sep);
                val canon: string = canonical_model_c_name(model_candidate);
                if str_len(canon) > 0 {
                    g_current_model = canon;
                }
            }
        }
    }

    val res_ret: string = resolve_self(ret_type);
    if str_len(res_ret) > 0 {
        StringStringMap.set(addr(g_function_return_types), arena, func_name, res_ret);
    } else {
        StringStringMap.set(addr(g_function_return_types), arena, func_name, str("void"));
    }

    if params == nil {
        StringStringMap.set(addr(g_function_param_types), arena, func_name, str(""));
        g_current_model = old_model;
        return;
    }

    mut sb_types: StringBuilder = StringBuilder.init(256);
    mut i: i32 = 0;
    loop {
        if i >= len(deref(params)) {
            break;
        }
        val param: string = params.data[i];

        if equals_c(param, "...") {
            if i > 0 {
                StringBuilder.append_c(addr(sb_types), ",");
            }
            StringBuilder.append_c(addr(sb_types), "...");
        } else {
            val colon_pos: i32 = find_char_from(param, ':', cast[usize](0));
            if colon_pos >= 0 {
                val param_type: string = strip(substr(param, colon_pos + 1, str_len(param) - colon_pos - 1));
                val res_type: string = resolve_self(param_type);
                if i > 0 {
                    StringBuilder.append_c(addr(sb_types), ",");
                }
                StringBuilder.append(addr(sb_types), res_type);
            }
        }
        i++;
    }
    val types_str: string = StringBuilder.to_string(addr(sb_types));
    StringBuilder.destroy(addr(sb_types));
    StringStringMap.set(addr(g_function_param_types), arena, func_name, types_str);
    g_current_model = old_model;
}

/// Infer the type of an expression (simplified type inference, non-recursive)
def infer_expression_type(expr: string): string {
    val trimmed: string = strip(expr);
    
    if str_len(trimmed) == 0 {
        return str("void");
    }
    
    // String literal
    if has_prefix(trimmed, str("\"")) and has_suffix(trimmed, str("\"")) {
        return str("ref char");
    }
    
    // Character literal
    if has_prefix(trimmed, str("'")) and has_suffix(trimmed, str("'")) {
        return str("char");
    }
    
    val first_char: char = get_char(trimmed, 0);

    if has_prefix(trimmed, str("(")) and has_suffix(trimmed, str(")")) {
        val inner_paren: string = strip(substring_se(trimmed, 1, str_len(trimmed) - 1));
        if str_len(inner_paren) > 0 {
            return infer_expression_type(inner_paren);
        }
    }

    if get_char(trimmed, 0) == '*' {
        val inner_name: string = strip(substring_se(trimmed, 1, cast[i32](str_len(trimmed))));
        if str_len(inner_name) > 0 {
            mut inner_type3: string = str("");
            if StringStringMap.contains(addr(g_var_types), inner_name) {
                inner_type3 = StringStringMap.get(addr(g_var_types), inner_name);
            } else {
                inner_type3 = infer_expression_type(inner_name);
            }
            if str_len(inner_type3) > 0 {
                if has_prefix(inner_type3, str("ref ")) {
                    return strip(substr(inner_type3, 4, str_len(inner_type3) - 4));
                }
                return inner_type3;
            }
        }
    }
    mut is_numeric: bool = false;
    
    if (first_char >= '0' and first_char <= '9') {
        is_numeric = true;
    } elif first_char == '-' {
        if str_len(trimmed) > 1 {
            val second_char: char = get_char(trimmed, 1);
            if (second_char >= '0' and second_char <= '9') or second_char == '.' {
                is_numeric = true;
            }
        }
    }

    if is_numeric {
        if find_char_from(trimmed, '.', cast[usize](0)) >= 0 {
            return str("f64");
        }
        return str("i32");
    }
    
    // Boolean literals
    if equals_c(trimmed, "true") or equals_c(trimmed, "false") {
        return str("bool");
    }
    
    // nil literal
    if equals_c(trimmed, "nil") {
        return str("ref void");
    }
    
    // Variable reference - look up in g_var_types
    if StringStringMap.contains(addr(g_var_types), trimmed) {
        mut var_type: string = StringStringMap.get(addr(g_var_types), trimmed);
        
        if g_current_type_mapping != nil and str_len(var_type) > 0 {
            if StringStringMap.contains(g_current_type_mapping, var_type) {
                var_type = StringStringMap.get(g_current_type_mapping, var_type);
            }
        }
        
        return var_type;
    }

    if has_prefix(trimmed, str("&")) {
        mut inner: string = strip(substr(trimmed, 1, str_len(trimmed) - 1));
        if str_len(inner) > 0 {
            if has_prefix(inner, str("(")) and has_suffix(inner, str(")")) {
                inner = strip(substr(inner, 1, str_len(inner) - 1));
            }
            mut inner_type: string = str("");
            if StringStringMap.contains(addr(g_var_types), inner) {
                inner_type = StringStringMap.get(addr(g_var_types), inner);
            } else {
                inner_type = infer_expression_type(inner);
            }
            if str_len(inner_type) > 0 {
                return concat(str("ref "), inner_type);
            }
        }
    }

    if has_prefix(trimmed, str("addr")) and str_len(trimmed) > 4 {
        val after_addr: i32 = 4;
        mut k: i32 = after_addr;
        loop {
            if k >= str_len(trimmed) {
                break;
            }
            val ws: char = get_char(trimmed, k);
            if ws != ' ' and ws != '\t' and ws != '\n' {
                break;
            }
            k++;
        }
        if k < str_len(trimmed) and get_char(trimmed, k) == '(' {
            val paren_start: i32 = k + 1;
            mut paren_end: i32 = paren_start;
            mut depth: i32 = 1;
            mut in_arg_str: bool = false;
            mut in_arg_char: bool = false;
            loop {
                if paren_end >= str_len(trimmed) or depth == 0 {
                    break;
                }
                val pc: char = get_char(trimmed, paren_end);
                if pc == '"' and !in_arg_char {
                    in_arg_str = !in_arg_str;
                } elif pc == '\'' and !in_arg_str {
                    in_arg_char = !in_arg_char;
                } elif !in_arg_str and !in_arg_char {
                    if pc == '(' {
                        depth++;
                    } elif pc == ')' {
                        depth--;
                    }
                }
                if depth > 0 {
                    paren_end++;
                }
            }
            if depth == 0 and paren_end == str_len(trimmed) - 1 {
                val inner_expr: string = strip(substring_se(trimmed, paren_start, paren_end));
                val inner_type: string = infer_expression_type(inner_expr);
                if str_len(inner_type) > 0 {
                    return concat(str("ref "), inner_type);
                }
            }
        }
    }

    if has_prefix(trimmed, str("deref")) and str_len(trimmed) > 5 {
        val after_deref: i32 = 5;
        mut k2: i32 = after_deref;
        loop {
            if k2 >= str_len(trimmed) {
                break;
            }
            val ws2: char = get_char(trimmed, k2);
            if ws2 != ' ' and ws2 != '\t' and ws2 != '\n' {
                break;
            }
            k2++;
        }
        if k2 < str_len(trimmed) and get_char(trimmed, k2) == '(' {
            val paren_start: i32 = k2 + 1;
            mut paren_end: i32 = paren_start;
            mut depth2: i32 = 1;
            mut in_arg_str2: bool = false;
            mut in_arg_char2: bool = false;
            loop {
                if paren_end >= str_len(trimmed) or depth2 == 0 {
                    break;
                }
                val pc2: char = get_char(trimmed, paren_end);
                if pc2 == '"' and !in_arg_char2 {
                    in_arg_str2 = !in_arg_str2;
                } elif pc2 == '\'' and !in_arg_str2 {
                    in_arg_char2 = !in_arg_char2;
                } elif !in_arg_str2 and !in_arg_char2 {
                    if pc2 == '(' {
                        depth2++;
                    } elif pc2 == ')' {
                        depth2--;
                    }
                }
                if depth2 > 0 {
                    paren_end++;
                }
            }
            if depth2 == 0 and paren_end == str_len(trimmed) - 1 {
                val inner_expr: string = strip(substring_se(trimmed, paren_start, paren_end));
                mut inner_type: string = str("");
                if StringStringMap.contains(addr(g_var_types), inner_expr) {
                    inner_type = StringStringMap.get(addr(g_var_types), inner_expr);
                } else {
                    inner_type = infer_expression_type(inner_expr);
                }
                if str_len(inner_type) > 0 {
                    if has_prefix(inner_type, str("ref ")) {
                        return strip(substr(inner_type, 4, str_len(inner_type) - 4));
                    }
                    return inner_type;
                }
            }
        }
    }

    // Function call
    val paren_idx: i32 = find_char_from(trimmed, '(', cast[usize](0));
    if paren_idx > 0 and has_suffix(trimmed, str(")")) {
        mut func_name: string = strip(substring_se(trimmed, 0, paren_idx));
        val args_start: i32 = paren_idx + 1;
        val args_end: i32 = str_len(trimmed) - 1;
        if args_end > args_start {
            val args_str: string = strip(substring_se(trimmed, args_start, args_end));
            validate_function_call(func_name, args_str, 0);
        } else {
            validate_function_call(func_name, str(""), 0);
        }

        mut lookup_name: string = func_name;
        val br_sq: i32 = find_char_from(func_name, '[', cast[usize](0));
        if br_sq > 0 {
            val br_sq_end: i32 = find_char_from(func_name, ']', cast[usize](br_sq));
            if br_sq_end > br_sq {
                val base_fn: string = strip(substring_se(func_name, 0, br_sq));
                val type_arg_raw: string = strip(substring_se(func_name, br_sq + 1, br_sq_end));
                mut mangled: string = base_fn;
                mut parts: list(string);
                mut sbt: StringBuilder = StringBuilder.init(64);
                mut pi: i32 = 0;
                loop {
                    if pi >= str_len(type_arg_raw) { break; }
                    val chp: char = get_char(type_arg_raw, pi);
                    if chp == ',' {
                        append(parts, strip(StringBuilder.to_string(addr(sbt))));
                        sbt = StringBuilder.init(64);
                    } else {
                        StringBuilder.append_char(addr(sbt), chp);
                    }
                    pi++;
                }
                val lastp: string = StringBuilder.to_string(addr(sbt));
                if str_len(strip(lastp)) > 0 { append(parts, strip(lastp)); }
                StringBuilder.destroy(addr(sbt));

                if len(parts) == 1 {
                    mangled = concat(mangled, str("__T_"));
                    mangled = concat(mangled, sanitize_c_identifier(map_axe_type_to_c(parts.data[0])));
                } elif len(parts) == 2 {
                    mangled = concat(mangled, str("__T2_"));
                    mangled = concat(mangled, sanitize_c_identifier(map_axe_type_to_c(parts.data[1])));
                    mangled = concat(mangled, str("__T_"));
                    mangled = concat(mangled, sanitize_c_identifier(map_axe_type_to_c(parts.data[0])));
                } else {
                    mut first: bool = true;
                    mut idxp: i32 = 0;
                    loop {
                        if idxp >= len(parts) { break; }
                        if !first { mangled = concat(mangled, str("__")); }
                        mangled = concat(mangled, sanitize_c_identifier(map_axe_type_to_c(parts.data[idxp])));
                        first = false;
                        idxp++;
                    }
                }

                lookup_name = mangled;
            }
        }

        if StringStringMap.contains(addr(g_function_prefixes), lookup_name) {
            lookup_name = StringStringMap.get(addr(g_function_prefixes), lookup_name);
        } elif StringStringMap.contains(addr(g_function_prefixes), func_name) {
            lookup_name = StringStringMap.get(addr(g_function_prefixes), func_name);
        }

        if equals_c(func_name, "cast") {
            if br_sq > 0 {
                if StringStringMap.contains(addr(g_function_return_types), lookup_name) {
                    return StringStringMap.get(addr(g_function_return_types), lookup_name);
                }
            }
            return str("cast_inferred_type");
        }

        if StringStringMap.contains(addr(g_function_return_types), lookup_name) {
            return StringStringMap.get(addr(g_function_return_types), lookup_name);
        }

        if equals_c(func_name, "len") { return str("i32"); }
        if equals_c(func_name, "sizeof") { return str("usize"); }
    }

    return str("");
}

/// Validate a function call's arguments against its signature
def validate_function_call(func_name: string, args_str: string, depth: i32) {
    // Skip validation if we're inside a non-matching when branch
    if g_inside_non_matching_when {
        return;
    }
    
    // Skip validation if we're inside a generic function but not in a specialization context
    // (i.e., processing the generic function definition itself, not a specialization)
    if g_current_type_mapping == nil and StringBoolMap.size(addr(g_current_function_type_params)) > 0 {
        return;
    }
    
    // Prevent infinite recursion
    if depth > 10 {
        return;
    }

    if is_builtin_function(func_name) {
        return;
    }

    if find_char_from(func_name, '"', cast[usize](0)) >= 0 or find_char_from(func_name, ',', cast[usize](0)) >= 0 {
        return;
    }

    if find_char_from(func_name, '&', cast[usize](0)) >= 0 {
        return;
    }

    if (find_char_from(func_name, ' ', cast[usize](0)) >= 0) 
        or (find_char_from(func_name, '\t', cast[usize](0)) >= 0) 
        or (find_char_from(func_name, '\n', cast[usize](0)) >= 0) 
        or (find_char_from(func_name, ')', cast[usize](0)) >= 0) 
        or (find_char_from(func_name, ']', cast[usize](0)) >= 0) {
        return;
    }

    if has_suffix(func_name, str("]")) and find_char_from(func_name, '[', cast[usize](0)) < 0 {
        val maybe_type: string = strip(replace_all(func_name, str("]"), str("")));
        if str_len(maybe_type) > 0 and (is_builtin_type(maybe_type) or StringStringMap.contains(addr(g_model_names), maybe_type) or is_symbol_imported(maybe_type)) {
            return;
        }
    }
    
    mut arg_list: list(string);
    mut sb_arg: StringBuilder = StringBuilder.init(256);
    mut paren_depth: i32 = 0;
    mut in_string: bool = false;
    mut in_char_literal: bool = false;
    mut i: i32 = 0;
    val args_len: i32 = cast[i32](str_len(args_str));

    loop {
        if i >= args_len {
            break;
        }
        val ch: char = get_char(args_str, i);
        
        if ch == '\'' and !in_string and !is_escaped_quote(args_str, i) {
            in_char_literal = !in_char_literal;
            StringBuilder.append_char(addr(sb_arg), ch);
        } elif ch == '\"' and !in_char_literal and !is_escaped_quote(args_str, i) {
            in_string = !in_string;
            StringBuilder.append_char(addr(sb_arg), ch);
        } elif !in_string and !in_char_literal {
            if ch == '(' {
                paren_depth = paren_depth + 1;
                StringBuilder.append_char(addr(sb_arg), ch);
            } elif ch == ')' {
                paren_depth = paren_depth - 1;
                StringBuilder.append_char(addr(sb_arg), ch);
            } elif ch == ',' and paren_depth == 0 {
                val built_arg: string = StringBuilder.to_string(addr(sb_arg));
                append(arg_list, strip(built_arg));
                sb_arg = StringBuilder.init(256);
            } else {
                StringBuilder.append_char(addr(sb_arg), ch);
            }
        } else {
            StringBuilder.append_char(addr(sb_arg), ch);
        }
        i++;
    }

    val final_arg: string = StringBuilder.to_string(addr(sb_arg));
    if str_len(strip(final_arg)) > 0 {
        append(arg_list, strip(final_arg));
    }

    mut arg_idx_nested: i32 = 0;
    loop {
        if arg_idx_nested >= len(arg_list) {
            break;
        }

        val arg_expr: string = arg_list.data[arg_idx_nested];

        // First check if this argument itself is a function call

        mut is_string_lit: bool = false;
        if has_prefix(arg_expr, str("\"")) {
            is_string_lit = true;
        } elif has_prefix(arg_expr, str("'")) {
            is_string_lit = true;
        }
        val arg_paren: i32 = find_char_from(arg_expr, '(', cast[usize](0));

        if !is_string_lit and arg_paren > 0 {
            val nested_func: string = strip(substring_se(arg_expr, 0, arg_paren));
            mut paren_depth2: i32 = 1;
            mut nested_args_end: i32 = -1;
            mut j: i32 = arg_paren + 1;
            mut in_string_check: bool = false;
            mut in_char_literal_check: bool = false;

            loop {
                if j >= str_len(arg_expr) {
                    break;
                }
                val ch: char = get_char(arg_expr, j);

                if ch == '\'' and !in_string_check and !is_escaped_quote(arg_expr, j) {
                    in_char_literal_check = !in_char_literal_check;
                } elif ch == '\"' and !in_char_literal_check and !is_escaped_quote(arg_expr, j) {
                    in_string_check = !in_string_check;
                } elif !in_string_check and !in_char_literal_check {
                    if ch == '(' {
                        paren_depth2 = paren_depth2 + 1;
                    } elif ch == ')' {
                        paren_depth2 = paren_depth2 - 1;
                        if paren_depth2 == 0 {
                            nested_args_end = j;
                            break;
                        }
                    }
                }
                j++;
            }

            if nested_args_end >= 0 {
                val nested_args_start: i32 = arg_paren + 1;
                if nested_args_end > nested_args_start {
                    val nested_args: string = strip(substring_se(arg_expr, nested_args_start, nested_args_end));
                    validate_function_call(nested_func, nested_args, depth + 1);
                } else {
                    validate_function_call(nested_func, str(""), depth + 1);
                }
            }
        }
        arg_idx_nested++;
    }

    mut lookup_name: string = func_name;
    if StringStringMap.contains(addr(g_function_prefixes), func_name) {
        lookup_name = StringStringMap.get(addr(g_function_prefixes), func_name);
    }

    val br_sq_v: i32 = find_char_from(func_name, '[', cast[usize](0));
    if br_sq_v > 0 {
        val br_sq_end_v: i32 = find_char_from(func_name, ']', cast[usize](br_sq_v));
        if br_sq_end_v > br_sq_v {
            val base_fn_v: string = strip(substring_se(func_name, 0, br_sq_v));
            val type_arg_raw_v: string = strip(substring_se(func_name, br_sq_v + 1, br_sq_end_v));
            mut mangled_v: string = base_fn_v;
            mut parts_v: list(string);
            mut sbtv: StringBuilder = StringBuilder.init(64);
            mut pvi: i32 = 0;
            loop {
                if pvi >= str_len(type_arg_raw_v) { break; }
                val chp_v: char = get_char(type_arg_raw_v, pvi);
                if chp_v == ',' {
                    append(parts_v, strip(StringBuilder.to_string(addr(sbtv))));
                    sbtv = StringBuilder.init(64);
                } else {
                    StringBuilder.append_char(addr(sbtv), chp_v);
                }
                pvi++;
            }
            val lastpv: string = StringBuilder.to_string(addr(sbtv));
            if str_len(strip(lastpv)) > 0 { append(parts_v, strip(lastpv)); }
            StringBuilder.destroy(addr(sbtv));

            if len(parts_v) == 1 {
                mangled_v = concat(mangled_v, str("__T_"));
                mangled_v = concat(mangled_v, sanitize_c_identifier(map_axe_type_to_c(parts_v.data[0])));
            } elif len(parts_v) == 2 {
                mangled_v = concat(mangled_v, str("__T2_"));
                mangled_v = concat(mangled_v, sanitize_c_identifier(map_axe_type_to_c(parts_v.data[1])));
                mangled_v = concat(mangled_v, str("__T_"));
                mangled_v = concat(mangled_v, sanitize_c_identifier(map_axe_type_to_c(parts_v.data[0])));
            } else {
                mut first_v: bool = true;
                mut idxpv: i32 = 0;
                loop {
                    if idxpv >= len(parts_v) { break; }
                    if !first_v { mangled_v = concat(mangled_v, str("__")); }
                    mangled_v = concat(mangled_v, sanitize_c_identifier(map_axe_type_to_c(parts_v.data[idxpv])));
                    first_v = false;
                    idxpv++;
                }
            }

            lookup_name = mangled_v;
        }
    }
    
    if has_prefix(func_name, str("cast")) {
        return;
    }

    val dot_pos: i32 = find_char_from(func_name, '.', cast[usize](0));
    if dot_pos > 0 {
        val base_name: string = strip(substring_se(func_name, 0, dot_pos));
        if is_symbol_imported(base_name) or StringStringMap.contains(addr(g_model_names), base_name) {
            return;
        }
    }
    
    val t_pos_lookup: i32 = find_substr(lookup_name, str("__T_"));
    if t_pos_lookup > 0 {
         val base_gen: string = substring_se(lookup_name, 0, t_pos_lookup);
         if StringBoolMap.contains(addr(g_generic_functions), base_gen) {
             lookup_name = base_gen;
         } else {
             if StringStringMap.contains(addr(g_function_prefixes), base_gen) {
                 val pref_base: string = StringStringMap.get(addr(g_function_prefixes), base_gen);
                 if StringBoolMap.contains(addr(g_generic_functions), pref_base) {
                     lookup_name = pref_base;
                 }
             }
         }
    }

    val dot_pos_lookup: i32 = find_char_from(func_name, '.', cast[usize](0));
    if dot_pos_lookup > 0 {
        val base_name_lookup: string = strip(substring_se(func_name, 0, dot_pos_lookup));
        if equals_c(base_name_lookup, "C") {
            return;
        }
    }
        
    if has_prefix(func_name, str("C."))
        or has_prefix(func_name, str("C ."))
        or has_prefix(lookup_name, str("C__")) {
        return;
    }
    
    if !StringStringMap.contains(addr(g_function_param_types), lookup_name) {
        if StringBoolMap.contains(addr(g_extern_functions), lookup_name)
            or StringBoolMap.contains(addr(g_generic_functions), lookup_name)
            or StringBoolMap.contains(addr(g_emitted_prototypes), lookup_name)
            or StringBoolMap.contains(addr(g_function_prefixes), lookup_name)
            or equals_c(lookup_name, "len_v") {
            return;
        }

        if StringBoolMap.contains(addr(g_overloads), lookup_name) {
            if len(arg_list) > 0 {
                val i2: i32 = 0;
                val arg_expr: string = arg_list.data[i2];
                val inferred_type: string = infer_expression_type(arg_expr);

                if str_len(inferred_type) > 0 and !equals_c(inferred_type, "T") {
                    if StringStringMap.contains(addr(g_overload_type_mappings), lookup_name) {
                        val allowed_types_str: string = StringStringMap.get(addr(g_overload_type_mappings), lookup_name);

                        mut allowed_types: list(string);
                        mut sb_type: StringBuilder = StringBuilder.init(128);
                        mut j: usize = 0;
                        loop {
                            if j >= str_len(allowed_types_str) {
                                break;
                            }
                            val ch: char = get_char(allowed_types_str, cast[i32](j));
                            if ch == ',' {
                                val built_type: string = StringBuilder.to_string(addr(sb_type));
                                if str_len(strip(built_type)) > 0 {
                                    append(allowed_types, strip(built_type));
                                }
                                sb_type = StringBuilder.init(128);
                            } else {
                                StringBuilder.append_char(addr(sb_type), ch);
                            }
                            j++;
                        }

                        val final_type: string = StringBuilder.to_string(addr(sb_type));
                        if str_len(strip(final_type)) > 0 {
                            append(allowed_types, strip(final_type));
                        }

                        mut found: bool = false;
                        mut k: i32 = 0;
                        loop {
                            if k >= len(allowed_types) {
                                break;
                            }
                            val allowed_type_c: string = map_axe_type_to_c(allowed_types.data[k]);
                            val inferred_type_c: string = map_axe_type_to_c(inferred_type);
                            if compare(allowed_type_c, inferred_type_c) == 0 {
                                found = true;
                                break;
                            }
                            k++;
                        }

                        if !found and !equals_c(inferred_type, "auto") {
                            print(get_short_filename(g_current_source_file));
                            print ":";
                            print(i32_to_string(g_current_line));
                            print ": error: overload '";
                            print func_name;
                            print "' does not support type '";
                            print inferred_type;
                            print "', allowed types are: ";
                            mut print_idx: i32 = 0;
                            loop {
                                if print_idx >= len(allowed_types) {
                                    break;
                                }
                                if print_idx > 0 {
                                    print ", ";
                                }
                                print(replace_all(allowed_types.data[print_idx], str("__"), str(".")));
                                print_idx++;
                            }
                            println "";
                            unsafe {
                                C.exit(1);
                            }
                        }
                    }
                }
            }
            return;
        }
    }
    
    val expected_types_str: string = StringStringMap.get(addr(g_function_param_types), lookup_name);
    if str_len(expected_types_str) == 0 {
        if len(arg_list) > 0 {
            val has_brace: bool = find_char_from(func_name, '{', cast[usize](0)) >= 0;
            if !has_prefix(func_name, str("__INTERPOLATED__")) and !has_brace {
                print(get_short_filename(g_current_source_file));
                print ":";
                print(i32_to_string(g_current_line));
                print ": error: function '";
                print func_name;
                print "' expects 0 argument(s), but ";
                print(i32_to_string(len(arg_list)));
                println " were provided";
                unsafe {
                    C.exit(1);
                }
            }
        }
        return;
    }
        
    mut expected_types: list(string);
    mut sb_type: StringBuilder = StringBuilder.init(128);
    mut j: usize = 0;
    loop {
        if j >= str_len(expected_types_str) {
            break;
        }
        val ch2: char = get_char(expected_types_str, cast[i32](j));
        if ch2 == ',' {
            val built_type: string = StringBuilder.to_string(addr(sb_type));
            if str_len(strip(built_type)) > 0 {
                append(expected_types, strip(built_type));
            }
            sb_type = StringBuilder.init(128);
        } else {
            StringBuilder.append_char(addr(sb_type), ch2);
        }
        j++;
    }

    val final_type: string = StringBuilder.to_string(addr(sb_type));
    if str_len(strip(final_type)) > 0 {
        append(expected_types, strip(final_type));
    }
    
    mut is_variadic: bool = false;
    if len(expected_types) > 0 {
        val last_type: string = expected_types.data[len(expected_types) - 1];
        if equals_c(last_type, "...") {
            is_variadic = true;
        }
    }
    
    mut arg_count_ok: bool = false;
    if is_variadic {
        if len(arg_list) >= len(expected_types) - 1 {
            arg_count_ok = true;
        }
    } else {
        if len(arg_list) == len(expected_types) {
            arg_count_ok = true;
        }
    }
    
    if !arg_count_ok {
        print(get_short_filename(g_current_source_file));
        print ":";
        print(i32_to_string(g_current_line));
        print ": error: function '";
        print func_name;
        if is_variadic {
            print "' expects at least ";
            print(i32_to_string(len(expected_types) - 1));
        } else {
            print "' expects ";
            print(i32_to_string(len(expected_types)));
        }
        print " argument(s), but ";
        print(i32_to_string(len(arg_list)));
        println " were provided";
        unsafe {
            C.exit(1);
        }
    }
    
    mut arg_idx: i32 = 0;

    loop {
        if arg_idx >= len(arg_list) {
            break;
        }
        
        if is_variadic and arg_idx >= len(expected_types) - 1 {
            arg_idx = arg_idx + 1;
            continue;
        }

        val arg_expr: string = arg_list.data[arg_idx];
        val inferred_type: string = infer_expression_type(arg_expr);
        val expected_type: string = expected_types.data[arg_idx];

        if equals_c(expected_type, "...") {
            arg_idx++;
            continue;
        }

        val old_model_for_validation: string = g_current_model;

        if find_substr(expected_type, str("Self")) >= 0 {
            if arg_idx == 0 and str_len(inferred_type) > 0 {
                if StringStringMap.contains(addr(g_model_names), inferred_type) {
                    g_current_model = inferred_type;
                } else {
                    val base_type: string = extract_base_type(inferred_type);
                    if StringStringMap.contains(addr(g_model_names), base_type) {
                        g_current_model = base_type;
                    }
                }
            }
        }

        val resolved_expected_type: string = resolve_self(expected_type);

        g_current_model = old_model_for_validation;

        val mapped_inferred: string = map_axe_type_to_c(inferred_type);
        val mapped_expected: string = map_axe_type_to_c(resolved_expected_type);
        mut is_generic_func: bool = StringBoolMap.contains(addr(g_generic_functions), func_name);

        if !is_generic_func and StringStringMap.contains(addr(g_function_prefixes), func_name) {
            val prefixed_func: string = StringStringMap.get(addr(g_function_prefixes), func_name);
            if StringBoolMap.contains(addr(g_generic_functions), prefixed_func) {
                is_generic_func = true;
            }
        }
        
        // If caller passed an address (addr(...)) but the parameter doesn't expect a ref, emit a specific error.
        if !is_generic_func and str_len(inferred_type) > 0 and has_prefix(inferred_type, str("ref ")) and !has_prefix(expected_type, str("ref ")) {
            print(get_short_filename(g_current_source_file));
            print ":";
            print(i32_to_string(g_current_line));
            print ": error: cannot pass address to non-ref parameter in call to '";
            print(replace_all(func_name, str("__"), str(".")));
            print "': argument ";
            print(i32_to_string(arg_idx + 1));
            print " expects type '";
            print resolved_expected_type;
            print "' but got '";
            print inferred_type;
            unsafe {
                C.exit(1);
            }
        }

        if !StringBoolMap.contains(addr(g_extern_functions), func_name) and !is_generic_func {
            if str_len(inferred_type) > 0 and !equals_c(inferred_type, "auto") and !types_match(mapped_inferred, mapped_expected) {
                if has_prefix(inferred_type, expected_type) {
                    arg_idx++;
                    continue;
                }
                print(get_short_filename(g_current_source_file));
                print ":";
                print(i32_to_string(g_current_line));
                print ": error: type mismatch in call to '";
                print(replace_all(func_name, str("__"), str(".")));
                print "': argument ";
                print(i32_to_string(arg_idx + 1));
                print " expects type '";
                print resolved_expected_type;
                print "' but got '";
                print inferred_type;
                println "'";
                unsafe {
                    C.exit(1);
                }
            }
        }
        arg_idx++;
    }
}

/// Recursively collect function signatures from AST
def collect_function_signatures(node: ref ASTNode, arena: ref Arena, prefix: string) {
    if node == nil {
        return;
    }
    
    if equals_c(node.node_type, "Function") {
        val func_name: string = node.data.function.name;
        val params: ref list(string) = node.data.function.params;
        val ret_type: string = node.data.function.return_type;
        
        mut reg_name: string = func_name;
        if str_len(prefix) > 0 {
            reg_name = concat(prefix, func_name);
        }
        
        register_function_signature(reg_name, params, ret_type, arena);
        
        if node.data.function.is_generic {
            StringBoolMap.set(addr(g_generic_functions), arena, reg_name, true);
            StringLongMap.set(addr(g_generic_function_asts), arena, reg_name, cast[i64](node));
            if StringStringMap.contains(addr(g_function_prefixes), reg_name) {
                val prefixed_name: string = StringStringMap.get(addr(g_function_prefixes), reg_name);
                StringBoolMap.set(addr(g_generic_functions), arena, prefixed_name, true);
                StringLongMap.set(addr(g_generic_function_asts), arena, prefixed_name, cast[i64](node));
            }
        }
    }
    
    if equals_c(node.node_type, "Extern") {
        val func_name: string = node.data.extern_node.function_name;
        val params: ref list(string) = node.data.extern_node.params;
        val ret_type: string = node.data.extern_node.return_type;
        val symbol: string = node.data.extern_node.symbol;

        register_function_signature(func_name, params, ret_type, arena);
        StringBoolMap.set(addr(g_extern_functions), arena, func_name, true);
        
        if str_len(symbol) > 0 {
            StringStringMap.set(addr(g_extern_aliases), arena, func_name, symbol);
        }
    }

    if equals_c(node.node_type, "Overload") {
        val func_name: string = node.data.overload_node.name;
        StringBoolMap.set(addr(g_overloads), arena, func_name, true);

        val type_names_ref: ref list(string) = node.data.overload_node.type_names;
        if type_names_ref != nil {
            mut type_list_sb: StringBuilder = StringBuilder.init(256);
            mut i_type: i32 = 0;
            val count: i32 = len(deref(type_names_ref));
            loop {
                if i_type >= count {
                    break;
                }
                val type_name: string = type_names_ref.data[i_type];
                if i_type > 0 {
                    StringBuilder.append_c(addr(type_list_sb), ",");
                }
                StringBuilder.append(addr(type_list_sb), type_name);
                i_type = i_type + 1;
            }
            val type_list_str: string = StringBuilder.to_string(addr(type_list_sb));
            StringStringMap.set(addr(g_overload_type_mappings), arena, func_name, type_list_str);
            StringBuilder.destroy(addr(type_list_sb));
        }
    }
    
    if equals_c(node.node_type, "Model") {
        val model_name: string = node.data.model_node.name;
        val old_model: string = g_current_model;
        g_current_model = model_name;

        if node.children != nil {
            val children: ref list(ASTNode) = node.children;
            mut i: i32 = 0;
            loop {
                if i >= len(deref(children)) {
                    break;
                }
                mut model_prefix: string = concat(model_name, str("."));
                collect_function_signatures(addr(children.data[i]), arena, model_prefix);
                i++;
            }
        }

        g_current_model = old_model;
        return;
    }

    if node.children != nil {
        val children: ref list(ASTNode) = node.children;
        mut i: i32 = 0;
        loop {
            if i >= len(deref(children)) {
                break;
            }
            collect_function_signatures(addr(children.data[i]), arena, prefix);
            i++;
        }
    }
}

/// Collect foreign types from Foreign nodes before processing functions
def collect_foreign_types(node: ref ASTNode) {
    if node == nil {
        return;
    }
    
    if equals_c(node.node_type, "Foreign") {
        val fnames: ref list(string) = node.data.foreign_node.type_names;
        if fnames != nil {
            mut arena_foreign: Arena = Arena.create(2560);
            mut fi: i32 = 0;
            loop {
                if fi >= len(deref(fnames)) {
                    break;
                }
                val type_name: string = fnames.data[fi];
                StringBoolMap.set(addr(g_foreign_types), addr(arena_foreign), type_name, true);
                fi++;
            }
        }
    }
    
    if node.children != nil {
        val children: ref list(ASTNode) = node.children;
        mut i: i32 = 0;
        loop {
            if i >= len(deref(children)) {
                break;
            }
            collect_foreign_types(addr(children.data[i]));
            i++;
        }
    }
}

/// Build the g_function_param_types map from the AST
def build_function_signatures(ast: ref ASTNode) {
    mut arena: Arena = Arena.create(1024 * 1024);
    collect_function_signatures(ast, addr(arena), str(""));
}

/// Collect external imports from AST recursively.
/// `current_platform` is "" for global or a platform name (e.g. "windows",
/// "posix", "linux", "macos", "freebsd", "openbsd", "netbsd",
/// "dragonflybsd") for platform-specific imports.
def collect_external_imports(node: ref ASTNode, current_platform: string, arena: ref Arena) {
    if node == nil {
        return;
    }
    
    if equals_c(node.node_type, "ExternalImport") {
        val header: string = node.data.external_import.header_file;
        
        if equals_c(current_platform, "") {
            if !StringList.contains(g_global_external_headers, header) {
                StringList.push(g_global_external_headers, arena, header);
            }
        } elif equals_c(current_platform, "windows") {
            if !StringList.contains(g_windows_external_headers, header) {
                StringList.push(g_windows_external_headers, arena, header);
            }
        } elif equals_c(current_platform, "posix") {
            if !StringList.contains(g_posix_external_headers, header) {
                StringList.push(g_posix_external_headers, arena, header);
            }
        } elif equals_c(current_platform, "linux") {
            if !StringList.contains(g_linux_external_headers, header) {
                StringList.push(g_linux_external_headers, arena, header);
            }
        } elif equals_c(current_platform, "macos") {
            if !StringList.contains(g_macos_external_headers, header) {
                StringList.push(g_macos_external_headers, arena, header);
            }
        } elif equals_c(current_platform, "freebsd") {
            if !StringList.contains(g_freebsd_external_headers, header) {
                StringList.push(g_freebsd_external_headers, arena, header);
            }
        } elif equals_c(current_platform, "openbsd") {
            if !StringList.contains(g_openbsd_external_headers, header) {
                StringList.push(g_openbsd_external_headers, arena, header);
            }
        } elif equals_c(current_platform, "netbsd") {
            if !StringList.contains(g_netbsd_external_headers, header) {
                StringList.push(g_netbsd_external_headers, arena, header);
            }
        } elif equals_c(current_platform, "dragonflybsd") {
            if !StringList.contains(g_dragonflybsd_external_headers, header) {
                StringList.push(g_dragonflybsd_external_headers, arena, header);
            }
        }
        return;
    }
    
    if equals_c(node.node_type, "Platform") {
        val platform_name: string = node.data.platform_node.platform_name;
        // Recurse into platform children with platform context
        if node.children != nil {
            val children: ref list(ASTNode) = node.children;
            mut i: i32 = 0;
            loop {
                if i >= len(deref(children)) {
                    break;
                }
                collect_external_imports(addr(children.data[i]), platform_name, arena);
                i++;
            }
        }
        return;
    }
    
    // Recurse into all children with current platform context
    if node.children != nil {
        val children: ref list(ASTNode) = node.children;
        mut i: i32 = 0;
        loop {
            if i >= len(deref(children)) {
                break;
            }
            collect_external_imports(addr(children.data[i]), current_platform, arena);
            i++;
        }
    }
}

/// Extract the base type name from a field type string.
/// For example: "ref string" -> "string", "i32*" -> "i32", "list(Token)" -> "Token"
def get_base_type_from_field(field_type: string): string {
    mut t: string = strip(field_type);
    
    if has_prefix(t, str("mut ")) {
        t = strip(substr(t, 4, str_len(t) - 4));
    }
    
    loop {
        if !has_prefix(t, str("ref ")) {
            break;
        }
        t = strip(substr(t, 4, str_len(t) - 4));
    }
    
    loop {
        if !has_prefix(t, str("& ")) {
            break;
        }
        t = strip(substr(t, 2, str_len(t) - 2));
    }
    
    if has_prefix(t, str("list(")) {
        mut paren_pos: i32 = 5;
        mut depth: i32 = 1;
        loop {
            if paren_pos >= str_len(t) {
                break;
            }
            val ch: char = get_char(t, paren_pos);
            if ch == '(' {
                depth = depth + 1;
            } elif ch == ')' {
                depth = depth - 1;
                if depth == 0 {
                    break;
                }
            }
            paren_pos = paren_pos + 1;
        }
        t = strip(substr(t, 5, paren_pos - 5));
    }
    
    val bracket_pos: i32 = find_substr(t, str("["));

    if bracket_pos >= 0 {
        t = strip(substr(t, 0, bracket_pos));
    }
    
    loop {
        if str_len(t) == 0 {
            break;
        }
        if get_char(t, str_len(t) - 1) != '*' {
            break;
        }
        t = strip(substr(t, 0, str_len(t) - 1));
    }
    
    return t;
}

/// Remove stray spaces around '.' in numeric C literals.
def normalize_numeric_dots(s: string): string {
    mut sb: StringBuilder = StringBuilder.init(str_len(s) + 16);
    val n: i32 = cast[i32](str_len(s));
    mut i: i32 = 0;

    loop {
        if i >= n {
            break;
        }

        val ch: char = get_char(s, i);

        if ch >= '0' and ch <= '9' and i + 1 < n and get_char(s, i + 1) == '.' {
            StringBuilder.append_char(addr(sb), ch);
            StringBuilder.append_char(addr(sb), '.');

            mut j: i32 = i + 2;
            loop {
                if j >= n {
                    break;
                }
                val cj: char = get_char(s, j);
                if cj != ' ' and cj != '\t' and cj != '\n' {
                    break;
                }
                j++;
            }

            if j < n and get_char(s, j) >= '0' and get_char(s, j) <= '9' {
                StringBuilder.append_char(addr(sb), get_char(s, j));
                i = j + 1;
                continue;
            } else {
                i = j;
                continue;
            }
        }

        StringBuilder.append_char(addr(sb), ch);
        i++;
    }

    val result: string = StringBuilder.to_string(addr(sb));
    StringBuilder.destroy(addr(sb));
    return result;
}

/// Check if a quote character at position `pos` is escaped by counting
/// consecutive backslashes before it. Returns true if odd number of backslashes
/// (meaning the quote is escaped), false if even number (quote is real delimiter).
def is_escaped_quote(expr: string, pos: i32): bool {
    mut backslash_count: i32 = 0;
    mut j: i32 = pos - 1;
    loop {
        if j < 0 {
            break;
        }
        if get_char(expr, j) != '\\' {
            break;
        }
        backslash_count = backslash_count + 1;
        j = j - 1;
    }
    // Odd number of backslashes means the quote is escaped
    return (backslash_count mod 2) == 1;
}

/// Replace a keyword with another string, but only when it's a standalone word
/// (not part of a larger identifier) and not inside a string literal.
def replace_keyword_outside_strings(expr: string, keyword: string, replacement: string): string {
    val expr_len: i32 = cast[i32](str_len(expr));
    val kw_len: i32 = cast[i32](str_len(keyword));
    if expr_len == 0 or kw_len == 0 {
        return expr;
    }
    
    mut sb: StringBuilder = StringBuilder.init(cast[usize](expr_len + 32));
    mut in_string: bool = false;
    mut in_char_literal: bool = false;
    mut i: i32 = 0;
    
    loop {
        if i >= expr_len {
            break;
        }
        
        val ch: char = get_char(expr, i);
        
        if ch == '\'' and !in_string and !is_escaped_quote(expr, i) {
            in_char_literal = !in_char_literal;
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }
        
        if ch == '"' and !in_char_literal and !is_escaped_quote(expr, i) {
            in_string = !in_string;
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }
        
        if in_string or in_char_literal {
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }
        
        mut matches: bool = true;
        if i + kw_len > expr_len {
            matches = false;
        } else {
            mut ki: i32 = 0;
            loop {
                if ki >= kw_len {
                    break;
                }
                if get_char(expr, i + ki) != get_char(keyword, ki) {
                    matches = false;
                    break;
                }
                ki = ki + 1;
            }
        }
        
        if matches {
            mut is_word: bool = true;
            
            if i > 0 {
                val prev: char = get_char(expr, i - 1);
                val prev_is_alpha_lower: bool = prev >= 'a' and prev <= 'z';
                val prev_is_alpha_upper: bool = prev >= 'A' and prev <= 'Z';
                val prev_is_digit: bool = prev >= '0' and prev <= '9';
                val prev_is_underscore: bool = prev == '_';
                if prev_is_alpha_lower or prev_is_alpha_upper or prev_is_digit or prev_is_underscore {
                    is_word = false;
                }
            }
            
            if is_word and i + kw_len < expr_len {
                val next: char = get_char(expr, i + kw_len);
                val next_is_alpha_lower: bool = next >= 'a' and next <= 'z';
                val next_is_alpha_upper: bool = next >= 'A' and next <= 'Z';
                val next_is_digit: bool = next >= '0' and next <= '9';
                val next_is_underscore: bool = next == '_';
                if next_is_alpha_lower or next_is_alpha_upper or next_is_digit or next_is_underscore {
                    is_word = false;
                }
            }
            
            if is_word {
                if i > 0 {
                    val prev_ch: char = get_char(expr, i - 1);
                    if prev_ch != ' ' {
                        StringBuilder.append_char(addr(sb), ' ');
                    }
                }
                StringBuilder.append(addr(sb), replacement);
                if i + kw_len < expr_len {
                    val next_ch: char = get_char(expr, i + kw_len);
                    if next_ch != ' ' {
                        StringBuilder.append_char(addr(sb), ' ');
                    }
                }
                i = i + kw_len;
                continue;
            }
        }
        
        StringBuilder.append_char(addr(sb), ch);
        i++;
    }
    
    val result: string = StringBuilder.to_string(addr(sb));
    StringBuilder.destroy(addr(sb));
    return result;
}

/// Replace all occurrences of `old_sub` with `replacement` but only when
/// they occur outside string and character literals.
def replace_substr_outside_strings(expr: string, old_sub: string, replacement: string): string {
    val expr_len: i32 = cast[i32](str_len(expr));
    val old_len: i32 = cast[i32](str_len(old_sub));
    if expr_len == 0 or old_len == 0 {
        return expr;
    }

    mut sb: StringBuilder = StringBuilder.init(cast[usize](expr_len + 32));
    mut in_string: bool = false;
    mut in_char_literal: bool = false;
    mut i: i32 = 0;

    loop {
        if i >= expr_len {
            break;
        }

        val ch: char = get_char(expr, i);

        if ch == '\'' and !in_string and !is_escaped_quote(expr, i) {
            in_char_literal = !in_char_literal;
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }

        if ch == '"' and !in_char_literal and !is_escaped_quote(expr, i) {
            in_string = !in_string;
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }

        if in_string or in_char_literal {
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }

        if i + old_len <= expr_len {
            val candidate: string = substring_se(expr, i, i + old_len);
            if compare(candidate, old_sub) == 0 {
                StringBuilder.append(addr(sb), replacement);
                i = i + old_len;
                continue;
            }
        }

        StringBuilder.append_char(addr(sb), ch);
        i++;
    }

    val result: string = StringBuilder.to_string(addr(sb));
    StringBuilder.destroy(addr(sb));
    return result;
}

/// Apply extern aliases to function calls in an expression.
/// Replaces function names like "my_malloc(" with their aliased C symbols like "malloc(".
def apply_extern_aliases(expr: string): string {
    mut result: string = expr;
    mut i: i32 = 0;
    loop {
        if i >= deref(g_extern_aliases.keys).len {
            break;
        }
    
        val func_name: string = deref(g_extern_aliases.keys).data[i];
        val symbol: string = deref(g_extern_aliases.values).data[i];
        if str_len(func_name) > 0 and str_len(symbol) > 0 {
            val func_call: string = concat(func_name, str("("));
            val symbol_call: string = concat(symbol, str("("));
            result = replace_substr_outside_strings(result, func_call, symbol_call);

            val func_call_with_space: string = concat(func_name, str(" ("));
            val symbol_call_with_space: string = concat(symbol, str(" ("));
            result = replace_substr_outside_strings(result, func_call_with_space, symbol_call_with_space);

            val func_call_with_spaces: string = concat(func_name, str("  ("));
            val symbol_call_with_spaces: string = concat(symbol, str("  ("));
            result = replace_substr_outside_strings(result, func_call_with_spaces, symbol_call_with_spaces);

            val func_call_with_more_spaces: string = concat(func_name, str("   ("));
            val symbol_call_with_more_spaces: string = concat(symbol, str("   ("));
            result = replace_substr_outside_strings(result, func_call_with_more_spaces, symbol_call_with_more_spaces);

            val func_name_space: string = concat(func_name, str(" "));
            val symbol_space: string = concat(symbol, str(" "));
            result = replace_substr_outside_strings(result, func_name_space, symbol_space);

            val func_name_comma: string = concat(func_name, str(","));
            val symbol_comma: string = concat(symbol, str(","));
            result = replace_substr_outside_strings(result, func_name_comma, symbol_comma);

            val func_name_semicolon: string = concat(func_name, str(";"));
            val symbol_semicolon: string = concat(symbol, str(";"));
            result = replace_substr_outside_strings(result, func_name_semicolon, symbol_semicolon);

            val func_name_rparen: string = concat(func_name, str(")"));
            val symbol_rparen: string = concat(symbol, str(")"));
            result = replace_substr_outside_strings(result, func_name_rparen, symbol_rparen);

            result = replace_function_name_outside_strings(result, func_name, symbol);
        }
        i++;
    }
    return result;
}

/// Replace a function name with its alias, handling word boundaries to avoid partial matches
/// This function replaces complete identifiers only, avoiding partial matches within other identifiers
def replace_function_name_outside_strings(expr: string, old_name: string, new_name: string): string {
    mut result: StringBuilder = StringBuilder.init(str_len(expr) + 64);
    mut i: i32 = 0;
    val expr_len: i32 = cast[i32](str_len(expr));
    val old_len: i32 = cast[i32](str_len(old_name));

    mut in_string: bool = false;
    mut in_char_literal: bool = false;

    loop {
        if i >= expr_len {
            break;
        }

        if get_char(expr, i) == '\'' and !in_string and !is_escaped_quote(expr, i) {
            in_char_literal = !in_char_literal;
            StringBuilder.append_char(addr(result), get_char(expr, i));
            i++;
            continue;
        }

        if get_char(expr, i) == '"' and !in_char_literal and !is_escaped_quote(expr, i) {
            in_string = !in_string;
            StringBuilder.append_char(addr(result), get_char(expr, i));
            i++;
            continue;
        }

        if in_string or in_char_literal {
            StringBuilder.append_char(addr(result), get_char(expr, i));
            i++;
            continue;
        }

        if i + old_len <= expr_len {
            mut match: bool = true;
            mut j: i32 = 0;
            loop {
                if j >= old_len {
                    break;
                }
                if get_char(expr, i + j) != get_char(old_name, j) {
                    match = false;
                    break;
                }
                j++;
            }

            if match {
                val start_ok: bool = (i == 0) or !is_ident_char(get_char(expr, i - 1));
                val end_ok: bool = (i + old_len >= expr_len) or !is_ident_char(get_char(expr, i + old_len));

                if start_ok and end_ok {
                    StringBuilder.append(addr(result), new_name);
                    i = i + old_len;
                    continue;
                }
            }
        }

        StringBuilder.append_char(addr(result), get_char(expr, i));
        i++;
    }

    val final_result: string = StringBuilder.to_string(addr(result));
    StringBuilder.destroy(addr(result));
    return final_result;
}

/// Check if a character is valid in an identifier
def is_ident_char(ch: char): bool {
    return (ch >= 'a' and ch <= 'z') or
           (ch >= 'A' and ch <= 'Z') or
           (ch >= '0' and ch <= '9') or
           ch == '_';
}

/// Rewrite member access on pointer-typed variables...
def rewrite_pointer_member_accesses(expr: string): string {
    mut sb: StringBuilder = StringBuilder.init(str_len(expr) + 64);
    mut i: i32 = 0;
    mut in_string: bool = false;
    mut in_char_literal: bool = false;
    val expr_len: i32 = cast[i32](str_len(expr));

    loop {
        if i >= expr_len {
            break;
        }

        val ch: char = get_char(expr, i);

        if ch == '\'' and !in_string and !is_escaped_quote(expr, i) {
            in_char_literal = !in_char_literal;
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }

        if ch == '"' and !in_char_literal and !is_escaped_quote(expr, i) {
            in_string = !in_string;
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }

        if !in_string and !in_char_literal and is_ident_start(ch) {
            val base_start: i32 = i;
            mut j: i32 = i;
            loop {
                if j >= expr_len {
                    break;
                }
                val cj: char = get_char(expr, j);
                if !is_token_char(cj) {
                    break;
                }
                j++;
            }

            val base: string = substring_se(expr, base_start, j);
            mut k: i32 = j;

            loop {
                if k >= expr_len {
                    break;
                }
                val ws: char = get_char(expr, k);
                if ws != ' ' and ws != '\t' and ws != '\n' {
                    break;
                }
                k++;
            }

            if k < expr_len and get_char(expr, k) == '.' {
                val start_slice: i32 = i;

                mut p: i32 = k + 1;
                loop {
                    if p >= expr_len {
                        break;
                    }
                    val ws2: char = get_char(expr, p);
                    if ws2 != ' ' and ws2 != '\t' and ws2 != '\n' {
                        break;
                    }
                    p++;
                }

                val member_start: i32 = p;
                mut m: i32 = p;
                if m < expr_len and is_ident_start(get_char(expr, m)) {
                    loop {
                        if m >= expr_len {
                            break;
                        }
                        val cm: char = get_char(expr, m);
                        if !is_token_char(cm) {
                            break;
                        }
                        m++;
                    }
                }
                val member: string = substring_se(expr, member_start, m);

                if str_len(member) > 0 {
                    val trimmed_base: string = strip(base);
                    mut is_pointer: bool = false;

                    if StringStringMap.contains(addr(g_var_types), trimmed_base) {
                        val raw_type: string = StringStringMap.get(addr(g_var_types), trimmed_base);
                        mut t: string = strip(raw_type);

                        if has_prefix(t, str("mut ")) {
                            t = strip(substr(t, 4, str_len(t) - 4));
                        }

                        if has_prefix(t, str("ref ")) or has_suffix(t, str("*")) {
                            is_pointer = true;
                        }
                    }

                    if is_pointer {
                        StringBuilder.append(addr(sb), trimmed_base);
                        StringBuilder.append_c(addr(sb), "->");
                        StringBuilder.append(addr(sb), member);
                        i = m;
                        continue;
                    }

                    // Not a pointer: fall through and emit original slice
                    val original: string = substring_se(expr, start_slice, m);
                    StringBuilder.append(addr(sb), original);
                    i = m;
                    continue;
                }
            }

            // Not a member access; emit the identifier as-is
            StringBuilder.append(addr(sb), base);
            i = j;
            continue;
        }

        StringBuilder.append_char(addr(sb), ch);
        i++;
    }

    val result: string = StringBuilder.to_string(addr(sb));
    StringBuilder.destroy(addr(sb));
    return result;
}

/// Register a single function name in g_function_prefixes
def register_function_prefix(full_name: string) {
    mut last_sep: i32 = -1;
    mut j: i32 = 0;
    val n: i32 = cast[i32](str_len(full_name));
    loop {
        if j + 1 >= n {
            break;
        }
        val c1: char = get_char(full_name, j);
        val c2: char = get_char(full_name, j + 1);
        if c1 == '_' and c2 == '_' {
            last_sep = j;
        }
        j++;
    }

    if last_sep >= 0 and last_sep + 2 < n {
        val short_name: string = substring_se(full_name, last_sep + 2, n);

        if str_len(short_name) > 0 {
            if !StringStringMap.contains(addr(g_function_prefixes), short_name) {
                mut arena: Arena = Arena.create(1256);
                StringStringMap.set(addr(g_function_prefixes), addr(arena), short_name, full_name);
            }
        }
        
        mut second_last_sep: i32 = -1;
        mut k: i32 = 0;
        loop {
            if k >= last_sep {
                break;
            }
            val c1b: char = get_char(full_name, k);
            val c2b: char = get_char(full_name, k + 1);
            if c1b == '_' and c2b == '_' {
                second_last_sep = k;
            }
            k++;
        }
        
        if second_last_sep >= 0 and second_last_sep + 2 < n {
            val partial_name: string = substring_se(full_name, second_last_sep + 2, n);
            if str_len(partial_name) > 0 {
                if !StringStringMap.contains(addr(g_function_prefixes), partial_name) {
                    mut arena2: Arena = Arena.create(1256);
                    StringStringMap.set(addr(g_function_prefixes), addr(arena2), partial_name, full_name);
                }
            }
        }
    }
}

/// Recursively collect function prefixes from an AST node
def collect_function_prefixes(node: ref ASTNode) {
    if node == nil {
        return;
    }
    
    if equals_c(node.node_type, "Function") {
        val fn_name: string = node.data.function.name;
        register_function_prefix(fn_name);
    }
    
    if equals_c(node.node_type, "Overload") {
        register_function_prefix(node.data.overload_node.name);
    }
    
    if node.children != nil {
        val children: ref list(ASTNode) = node.children;
        mut i: i32 = 0;
        loop {
            if i >= len(deref(children)) {
                break;
            }
            collect_function_prefixes(addr(children.data[i]));
            i++;
        }
    }
}

/// Build the g_function_prefixes map from the AST
def build_function_prefixes(ast: ref ASTNode) {
    StringStringMap.clear(addr(g_function_prefixes));
    collect_function_prefixes(ast);
}

/// Map Axe type to C type
def map_type(axe_type: string): string {
    if StringStringMap.contains(addr(g_type_mappings), axe_type) {
        return StringStringMap.get(addr(g_type_mappings), axe_type);
    }
    return axe_type;
}

def canonical_model_c_name(name: string): string {
    if str_contains_c(name, "__") {
        if StringStringMap.contains(addr(g_model_names), name) {
            return StringStringMap.get(addr(g_model_names), name);
        }
    }

    if str_len(g_current_module_prefix) > 0 {
        val prefixed: string = concat(concat(g_current_module_prefix, str("__")), name);
        if StringStringMap.contains(addr(g_model_names), prefixed) {
            return StringStringMap.get(addr(g_model_names), prefixed);
        }
        
        val parent_prefix: string = get_prefix_from_mangled(g_current_module_prefix);
        if str_len(parent_prefix) > 0 {
            val prefixed2: string = concat(concat(parent_prefix, str("__")), name);
            if StringStringMap.contains(addr(g_model_names), prefixed2) {
                return StringStringMap.get(addr(g_model_names), prefixed2);
            }
        }
    }

    if StringStringMap.contains(addr(g_model_names), name) {
        return StringStringMap.get(addr(g_model_names), name);
    }
    return str("");
}

/// Map Axe type to C type (comprehensive version for model fields)
def map_axe_type_to_c(axe_type: string): string {
    val res_self: string = resolve_self(axe_type);
    mut result: string = strip(res_self);
    
    if g_current_type_mapping != nil and StringStringMap.contains(g_current_type_mapping, result) {
        result = StringStringMap.get(g_current_type_mapping, result);
    } elif StringBoolMap.contains(addr(g_current_function_type_params), result) {
        return result; 
    }

    if find_char_from(result, '$', cast[usize](0)) >= 0 {
        mut out_sb: StringBuilder = StringBuilder.init(str_len(result) + 32);
        mut i: i32 = 0;
        val n: i32 = cast[i32](str_len(result));
        loop {
            if i >= n {
                break;
            }
            val ch: char = get_char(result, i);
            if ch == '$' and i + 1 < n and get_char(result, i + 1) == '(' {
                mut j: i32 = i + 2;
                mut ident_sb: StringBuilder = StringBuilder.init(32);
                loop {
                    if j >= n {
                        break;
                    }
                    val cj: char = get_char(result, j);
                    if cj == ')' {
                        break;
                    }
                    StringBuilder.append_char(addr(ident_sb), cj);
                    j++;
                }
                val ident: string = strip(StringBuilder.to_string(addr(ident_sb)));
                StringBuilder.destroy(addr(ident_sb));
                if str_len(ident) > 0 {
                    StringBuilder.append(addr(out_sb), str("struct "));
                    StringBuilder.append(addr(out_sb), ident);
                }
                i = j + 1;
                continue;
            }
            StringBuilder.append_char(addr(out_sb), ch);
            i++;
        }
        result = StringBuilder.to_string(addr(out_sb));
        StringBuilder.destroy(addr(out_sb));
    }
    
    if has_prefix(result, str("mut ")) {
        result = strip(substr(result, 4, str_len(result) - 4));
        return map_axe_type_to_c(result);
    }

    if has_prefix(result, str("val ")) {
        val base: string = strip(substr(result, 4, str_len(result) - 4));
        val mapped_base: string = map_axe_type_to_c(base);
        return concat(str("const "), mapped_base);
    }
    
    val result_len: i32 = cast[i32](str_len(result));
    if has_prefix(result, str("list")) {
        mut paren_pos: i32 = 4;
        loop {
            if paren_pos >= result_len { break; }
            val ch: char = get_char(result, paren_pos);
            if ch == '(' { break; }
            if ch != ' ' and ch != '\t' and ch != '\n' {
                paren_pos = -1;
                break;
            }
            paren_pos++;
        }
        
        if paren_pos > 0 and paren_pos < result_len {
            val start_pos: i32 = paren_pos + 1;
            mut depth: i32 = 1;
            mut end_pos: i32 = start_pos;
            loop {
                if end_pos >= result_len {
                    break;
                }
                val ch2: char = get_char(result, end_pos);
                if ch2 == '(' {
                    depth++;
                } elif ch2 == ')' {
                    depth--;
                    if depth == 0 {
                        break;
                    }
                }
                end_pos++;
            }
            if depth == 0 {
                val element_type: string = strip(substring_se(result, start_pos, end_pos));
                val mapped_elem: string = map_axe_type_to_c(element_type);
                
                if !StringBoolMap.contains(addr(g_current_function_type_params), mapped_elem) {
                    add_list_element(mapped_elem);
                } else {
                    return str("void");
                }
                val sanitized: string = sanitize_list_element_name(mapped_elem);
                
                mut sb_struct: StringBuilder = StringBuilder.init(str_len(sanitized) + 16);
                StringBuilder.append_c(addr(sb_struct), "__list_");
                StringBuilder.append(addr(sb_struct), sanitized);
                StringBuilder.append_c(addr(sb_struct), "_t");
                val struct_name: string = StringBuilder.to_string(addr(sb_struct));
                StringBuilder.destroy(addr(sb_struct));
                return struct_name;
            }
        }
    }
    
    if has_suffix(result, str("[999]")) {
        val element_type: string = strip(substring_se(result, 0, str_len(result) - 5));
        val mapped_elem: string = map_axe_type_to_c(element_type);
        add_list_element(mapped_elem);
        val sanitized: string = sanitize_list_element_name(mapped_elem);
        
        mut sb_struct: StringBuilder = StringBuilder.init(str_len(sanitized) + 10);
        StringBuilder.append_c(addr(sb_struct), "__list_");
        StringBuilder.append(addr(sb_struct), sanitized);
        StringBuilder.append_c(addr(sb_struct), "_t");
        val struct_name: string = StringBuilder.to_string(addr(sb_struct));
        StringBuilder.destroy(addr(sb_struct));
        return struct_name;
    }
    
    if has_prefix(result, str("ref ")) {
        val base_type: string = strip(substr(result, 4, str_len(result) - 4));
        val mapped_base: string = map_axe_type_to_c(base_type);
        return concat(mapped_base, str("*"));
    }
    
    if has_suffix(result, str("*")) {
        val base_type: string = strip(substring_se(result, 0, str_len(result) - 1));
        val mapped_base: string = map_axe_type_to_c(base_type);
        return concat(mapped_base, str("*"));
    }
    
    val br_idx: i32 = find_char_from(result, '[', cast[usize](0));
    if br_idx >= 0 and find_char_from(result, ']', cast[usize](br_idx)) >= 0 {
        val base_raw: string = strip(substring_se(result, 0, br_idx));
        val suffix: string = substring_se(result, br_idx, cast[i32](str_len(result)));
        if str_len(base_raw) > 0 {
            val mapped_base2: string = map_axe_type_to_c(base_raw);
            return concat(mapped_base2, suffix);
        }
    }
    
    if StringStringMap.contains(addr(g_type_mappings), result) {
        return StringStringMap.get(addr(g_type_mappings), result);
    }

    if StringStringMap.contains(addr(g_model_names), result) {
        return StringStringMap.get(addr(g_model_names), result);
    }

    if g_current_type_mapping != nil and StringStringMap.contains(g_current_type_mapping, result) {
        val concrete_type: string = StringStringMap.get(g_current_type_mapping, result);
        return map_axe_type_to_c(concrete_type);
    }

    return result;
}

/// Sanitize a C element type string to be used as part of a generated list type name
def sanitize_list_element_name(c_elem: string): string {
    mut sb: StringBuilder = StringBuilder.init(str_len(c_elem) + 32);
    mut i: i32 = 0;
    val n_len: i32 = cast[i32](str_len(c_elem));
    loop {
        if i >= n_len {
            break;
        }
        val ch: char = get_char(c_elem, i);
        if ch == '*' {
            StringBuilder.append_c(addr(sb), "_ptr");
        } elif ch == ' ' {
            StringBuilder.append_c(addr(sb), "_");
        } elif ch == '(' {
            StringBuilder.append_c(addr(sb), "_");
        } elif ch == ')' {
            StringBuilder.append_c(addr(sb), "_");
        } elif ch == '<' {
            StringBuilder.append_c(addr(sb), "_lt_");
        } elif ch == '>' {
            StringBuilder.append_c(addr(sb), "_gt_");
        } elif ch == ',' {
            StringBuilder.append_c(addr(sb), "_comma_");
        } else {
            StringBuilder.append_char(addr(sb), ch);
        }
        i++;
    }
    val result: string = StringBuilder.to_string(addr(sb));
    StringBuilder.destroy(addr(sb));
    return result;
}

/// Collapse all duplicate segments in the specified string expression.
def collapse_duplicate_segments(s: string): string {
    mut current: string = s;
    loop {
        val next: string = collapse_duplicate_segments_once(current);
        if compare(next, current) == 0 {
            return current;
        }
        current = next;
    }
}

/// Check if a type string indicates a pointer/reference type
def is_pointer_type(type_str: string): bool {
    mut t: string = strip(type_str);
    if has_prefix(t, str("mut ")) {
        t = strip(substr(t, 4, str_len(t) - 4));
    }
    if has_prefix(t, str("ref ")) or has_suffix(t, str("*")) {
        return true;
    }
    return false;
}

/// Extract the base model/struct name from a type string.
/// For example: "ref ParserContext" -> "ParserContext", "list(Token)*" -> "list(Token)"
def extract_base_model_name(type_str: string): string {
    mut t: string = strip(type_str);
    if has_prefix(t, str("mut ")) {
        t = strip(substr(t, 4, str_len(t) - 4));
    }
    if has_prefix(t, str("ref ")) {
        t = strip(substr(t, 4, str_len(t) - 4));
    }
    loop {
        if str_len(t) == 0 {
            break;
        }
        if get_char(t, str_len(t) - 1) != '*' {
            break;
        }
        t = strip(substr(t, 0, str_len(t) - 1));
    }
    return t;
}

/// Look up a field's type from g_field_types by trying various model prefixes.
/// Returns the field type if found, or empty string if not found.
def lookup_field_type(model_name: string, field_name: string): string {
    mut sb_key: StringBuilder = StringBuilder.init(str_len(model_name) + str_len(field_name) + 1);
    StringBuilder.append(addr(sb_key), model_name);
    StringBuilder.append_c(addr(sb_key), ".");
    StringBuilder.append(addr(sb_key), field_name);
    val field_key: string = StringBuilder.to_string(addr(sb_key));
    StringBuilder.destroy(addr(sb_key));

    if StringStringMap.contains(addr(g_field_types), field_key) {
        return StringStringMap.get(addr(g_field_types), field_key);
    }
    
    if StringStringMap.contains(addr(g_model_names), model_name) {
        val canonical_name: string = StringStringMap.get(addr(g_model_names), model_name);
        mut sb_canon: StringBuilder = StringBuilder.init(str_len(canonical_name) + str_len(field_name) + 1);
        StringBuilder.append(addr(sb_canon), canonical_name);
        StringBuilder.append_c(addr(sb_canon), ".");
        StringBuilder.append(addr(sb_canon), field_name);
        val canon_key: string = StringBuilder.to_string(addr(sb_canon));
        StringBuilder.destroy(addr(sb_canon));
        if StringStringMap.contains(addr(g_field_types), canon_key) {
            return StringStringMap.get(addr(g_field_types), canon_key);
        }
    }
    
    if has_prefix(model_name, str("list(")) or has_prefix(model_name, str("__list_")) {
        if equals_c(field_name, "data") {
            return str("ref void");
        }
    }

    return str("");
}

/// Check if a field is a pointer type by looking it up in g_pointer_fields
def is_pointer_field(model_name: string, field_name: string): bool {
    mut sb_fkey: StringBuilder = StringBuilder.init(str_len(model_name) + str_len(field_name) + 1);
    StringBuilder.append(addr(sb_fkey), model_name);
    StringBuilder.append_c(addr(sb_fkey), ".");
    StringBuilder.append(addr(sb_fkey), field_name);
    val field_key: string = StringBuilder.to_string(addr(sb_fkey));
    StringBuilder.destroy(addr(sb_fkey));
    if StringBoolMap.contains(addr(g_pointer_fields), field_key) {
        return true;
    }
    
    if StringStringMap.contains(addr(g_model_names), model_name) {
        val canonical_name: string = StringStringMap.get(addr(g_model_names), model_name);
        mut sb_ckey: StringBuilder = StringBuilder.init(str_len(canonical_name) + str_len(field_name) + 1);
        StringBuilder.append(addr(sb_ckey), canonical_name);
        StringBuilder.append_c(addr(sb_ckey), ".");
        StringBuilder.append(addr(sb_ckey), field_name);
        val canon_key: string = StringBuilder.to_string(addr(sb_ckey));
        StringBuilder.destroy(addr(sb_ckey));
        if StringBoolMap.contains(addr(g_pointer_fields), canon_key) {
            return true;
        }
    }
    
    if has_prefix(model_name, str("list(")) or has_prefix(model_name, str("__list_")) {
        if equals_c(field_name, "data") {
            return true;
        }
    }
    return false;
}

/// Parse an identifier from expr starting at position pos.
/// Updates pos to point past the identifier.
def parse_ident_at(expr: string, pos: ref i32): string {
    val expr_len: i32 = cast[i32](str_len(expr));
    if deref(pos) >= expr_len {
        return str("");
    }
    val ch: char = get_char(expr, deref(pos));
    if !is_ident_start(ch) {
        return str("");
    }
    val start: i32 = deref(pos);
    loop {
        if deref(pos) >= expr_len {
            break;
        }
        val c: char = get_char(expr, deref(pos));
        if !is_token_char(c) {
            break;
        }
        deref(pos) = deref(pos) + 1;
    }
    return substring_se(expr, start, deref(pos));
}

/// Skip whitespace in expr starting at position pos.
def skip_ws_at(expr: string, pos: ref i32) {
    val expr_len: i32 = cast[i32](str_len(expr));
    loop {
        if deref(pos) >= expr_len {
            break;
        }
        val c: char = get_char(expr, deref(pos));
        if c != ' ' and c != '\t' and c != '\n' {
            break;
        }
        deref(pos) = deref(pos) + 1;
    }
}

/// Rewrite member access operators from `.` to `->` when the base identifier
/// is known to be a pointer type (ref or pointer) according to g_var_types.
/// Also handles nested member accesses by tracking types through the chain.
/// For example, if `ctx` has type `ref ParserContext` and `tokens` field has type `ref list(Token)`,
/// then `ctx.tokens.data` becomes `ctx->tokens->data`.
def rewrite_ref_member_access(expr: string): string {
    mut sb: StringBuilder = StringBuilder.init(str_len(expr) + 64);
    mut pos: i32 = 0;
    mut in_string: bool = false;
    mut in_char_literal: bool = false;
    val expr_len: i32 = cast[i32](str_len(expr));

    loop {
        if pos >= expr_len {
            break;
        }

        val ch: char = get_char(expr, pos);

        if ch == '\'' and !in_string and !is_escaped_quote(expr, pos) {
            in_char_literal = !in_char_literal;
            StringBuilder.append_char(addr(sb), ch);
            pos = pos + 1;
            continue;
        }

        if ch == '"' and !in_char_literal and !is_escaped_quote(expr, pos) {
            in_string = !in_string;
            StringBuilder.append_char(addr(sb), ch);
            pos = pos + 1;
            continue;
        }

        if in_string or in_char_literal {
            StringBuilder.append_char(addr(sb), ch);
            pos = pos + 1;
            continue;
        }

        if is_ident_start(ch) {
            val ident: string = parse_ident_at(expr, addr(pos));
            
            mut lookahead: i32 = pos;
            skip_ws_at(expr, addr(lookahead));
            
            mut is_member_chain: bool = false;
            if lookahead < expr_len and get_char(expr, lookahead) == '.' {
                if lookahead + 1 < expr_len and get_char(expr, lookahead + 1) != '>' {
                    is_member_chain = true;
                }
            } elif lookahead + 1 < expr_len and get_char(expr, lookahead) == '-' and get_char(expr, lookahead + 1) == '>' {
                is_member_chain = true;
            }
            
            if is_member_chain {
                mut chain: string = ident;
                mut is_pointer: bool = false;
                mut current_type: string = str("");
                
                if StringStringMap.contains(addr(g_var_types), ident) {
                    current_type = StringStringMap.get(addr(g_var_types), ident);
                    if is_pointer_type(current_type) {
                        is_pointer = true;
                    }
                }
                
                mut base_model_name: string = extract_base_model_name(current_type);
                
                pos = lookahead;
                loop {
                    if pos >= expr_len {
                        break;
                    }
                    
                    skip_ws_at(expr, addr(pos));
                    
                    if pos >= expr_len {
                        break;
                    }
                    
                    mut has_dot: bool = false;
                    mut has_arrow: bool = false;
                    if get_char(expr, pos) == '.' {
                        if pos + 1 < expr_len and get_char(expr, pos + 1) == '>' {
                            break;
                        }
                        has_dot = true;
                    } elif pos + 1 < expr_len and get_char(expr, pos) == '-' and get_char(expr, pos + 1) == '>' {
                        has_arrow = true;
                    }
                    
                    if !has_dot and !has_arrow {
                        break;
                    }
                    
                    if has_arrow {
                        pos = pos + 2;
                    } else {
                        pos = pos + 1;
                    }
                    
                    skip_ws_at(expr, addr(pos));
                    
                    val field_name: string = parse_ident_at(expr, addr(pos));
                    if str_len(field_name) == 0 {
                        if has_arrow {
                            chain = concat(chain, str("->"));
                        } else {
                            if is_pointer {
                                chain = concat(chain, str("->"));
                            } else {
                                chain = concat(chain, str("."));
                            }
                        }
                        break;
                    }
                    
                    mut array_suffix: string = str("");
                    skip_ws_at(expr, addr(pos));
                    if pos < expr_len and get_char(expr, pos) == '[' {
                        val bracket_start: i32 = pos;
                        pos = pos + 1;
                        mut depth: i32 = 1;
                        loop {
                            if pos >= expr_len or depth == 0 {
                                break;
                            }
                            val bc: char = get_char(expr, pos);
                            if bc == '[' {
                                depth = depth + 1;
                            } elif bc == ']' {
                                depth = depth - 1;
                            }
                            pos = pos + 1;
                        }
                        val bracket_content: string = substring_se(expr, bracket_start + 1, pos - 1);
                        val processed_bracket: string = rewrite_ref_member_access(bracket_content);
                        array_suffix = concat(str("["), processed_bracket);
                        array_suffix = concat(array_suffix, str("]"));
                    }
                    
                    mut op: string = str(".");
                    if has_arrow {
                        op = str("->");
                    } elif is_pointer {
                        op = str("->");
                    }
                    
                    chain = concat(chain, op);
                    chain = concat(chain, field_name);
                    chain = concat(chain, array_suffix);
                    
                    val had_array_access: bool = str_len(array_suffix) > 0;
                    if had_array_access {
                        is_pointer = false;
                    }
                    
                    if str_len(base_model_name) > 0 {
                        val field_type: string = lookup_field_type(base_model_name, field_name);
                        if str_len(field_type) > 0 {
                            if is_pointer_type(field_type) {
                                if !had_array_access {
                                    is_pointer = true;
                                }
                            } else {
                                is_pointer = false;
                            }
                            base_model_name = extract_base_model_name(field_type);
                        } elif is_pointer_field(base_model_name, field_name) {
                            if !had_array_access {
                                is_pointer = true;
                            }
                            base_model_name = str("");
                        } else {
                            if !had_array_access {
                                if equals_c(field_name, "children") or
                                   equals_c(field_name, "params") or equals_c(field_name, "body") {
                                    is_pointer = true;
                                } else {
                                    is_pointer = false;
                                }
                            }
                            base_model_name = str("");
                        }
                    } else {
                        if !had_array_access {
                            if equals_c(field_name, "children") or
                               equals_c(field_name, "params") or equals_c(field_name, "body") {
                                is_pointer = true;
                            } else {
                                is_pointer = false;
                            }
                        }
                    }
                }
                
                StringBuilder.append(addr(sb), chain);
                continue;
            }
            
            // Not a member access chain, just output the identifier
            StringBuilder.append(addr(sb), ident);
            continue;
        }

        StringBuilder.append_char(addr(sb), ch);
        pos = pos + 1;
    }

    val result: string = StringBuilder.to_string(addr(sb));
    StringBuilder.destroy(addr(sb));
    return result;
}

/// Sanitize a string to be a valid C identifier part (replacing spaces, *, etc. with _)
def sanitize_c_identifier(name: string): string {
    mut sb: StringBuilder = StringBuilder.init(str_len(name) + 16);
    mut i: i32 = 0;
    val len: i32 = cast[i32](str_len(name));
    loop {
        if i >= len { break; }
        val ch: char = get_char(name, i);
        if (ch >= 'a' and ch <= 'z') or (ch >= 'A' and ch <= 'Z') or (ch >= '0' and ch <= '9') or ch == '_' {
            StringBuilder.append_char(addr(sb), ch);
        } else {
            StringBuilder.append_char(addr(sb), '_');
        }
        i++;
    }
    val result: string = StringBuilder.to_string(addr(sb));
    StringBuilder.destroy(addr(sb));
    return result;
}

/// Rewrite generic function calls of the form `func[Type](args)` into
/// specialized function names like `func__T_Type(args)`.
def rewrite_generic_calls(expr: string): string {
    mut sb: StringBuilder = StringBuilder.init(str_len(expr) + 128);
    mut i: i32 = 0;
    mut in_string: bool = false;
    mut in_char_literal: bool = false;
    val expr_len: i32 = cast[i32](str_len(expr));

    loop {
        if i >= expr_len {
            break;
        }

        val ch: char = get_char(expr, i);

        if ch == '\'' and !in_string and !is_escaped_quote(expr, i) {
            in_char_literal = !in_char_literal;
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }

        if ch == '"' and !in_char_literal and !is_escaped_quote(expr, i) {
            in_string = !in_string;
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }

        if in_string or in_char_literal {
            StringBuilder.append_char(addr(sb), ch);
            i++;
            continue;
        }

        if is_ident_start(ch) {
            val ident_start: i32 = i;
            mut j: i32 = i;
            loop {
                if j >= expr_len {
                    break;
                }
                val cj: char = get_char(expr, j);
                if !is_token_char(cj) {
                    break;
                }
                j = j + 1;
            }
            val ident: string = substring_se(expr, ident_start, j);
            
            if equals_c(ident, "cast") {
                StringBuilder.append(addr(sb), ident);
                i = j;
                continue;
            }
            
            mut k: i32 = j;
            loop {
                if k >= expr_len {
                    break;
                }
                val ws: char = get_char(expr, k);
                if ws != ' ' and ws != '\t' and ws != '\n' {
                    break;
                }
                k = k + 1;
            }
            
            if k < expr_len and get_char(expr, k) == '[' {
                k = k + 1;
                val type_start: i32 = k;
                
                mut bracket_depth: i32 = 1;
                loop {
                    if k >= expr_len or bracket_depth == 0 {
                        break;
                    }
                    val tc: char = get_char(expr, k);
                    if tc == '[' {
                        bracket_depth = bracket_depth + 1;
                    } elif tc == ']' {
                        bracket_depth = bracket_depth - 1;
                    }
                    if bracket_depth > 0 {
                        k = k + 1;
                    }
                }
                
                val type_end: i32 = k;
                val type_arg_raw: string = strip(substring_se(expr, type_start, type_end));
                
                if k < expr_len and get_char(expr, k) == ']' {
                    k = k + 1;
                    
                    loop {
                        if k >= expr_len {
                            break;
                        }
                        val ws2: char = get_char(expr, k);
                        if ws2 != ' ' and ws2 != '\t' and ws2 != '\n' {
                            break;
                        }
                        k = k + 1;
                    }
                    
                    if k < expr_len and get_char(expr, k) == '(' {
                         mut base_ident: string = ident;
                         if StringStringMap.contains(addr(g_function_prefixes), ident) {
                             base_ident = StringStringMap.get(addr(g_function_prefixes), ident);
                         }
                         
                        mut parts: list(string);
                        mut sbt: StringBuilder = StringBuilder.init(64);
                        mut pidx: i32 = 0;
                        mut depth_p: i32 = 0;
                        mut in_p_str: bool = false;
                        mut in_p_char: bool = false;
                        loop {
                            if pidx >= str_len(type_arg_raw) { break; }
                            val pc: char = get_char(type_arg_raw, pidx);
                            if pc == '"' and !in_p_char and !is_escaped_quote(type_arg_raw, pidx) {
                                in_p_str = !in_p_str;
                                StringBuilder.append_char(addr(sbt), pc);
                            } elif pc == '\'' and !in_p_str and !is_escaped_quote(type_arg_raw, pidx) {
                                in_p_char = !in_p_char;
                                StringBuilder.append_char(addr(sbt), pc);
                            } elif !in_p_str and !in_p_char {
                                if pc == '(' { depth_p = depth_p + 1; StringBuilder.append_char(addr(sbt), pc); }
                                elif pc == ')' { depth_p = depth_p - 1; StringBuilder.append_char(addr(sbt), pc); }
                                elif pc == ',' and depth_p == 0 {
                                    append(parts, strip(StringBuilder.to_string(addr(sbt))));
                                    sbt = StringBuilder.init(64);
                                } else { StringBuilder.append_char(addr(sbt), pc); }
                            } else { StringBuilder.append_char(addr(sbt), pc); }
                            pidx++;
                        }
                        val lastp: string = StringBuilder.to_string(addr(sbt));
                        if str_len(strip(lastp)) > 0 { append(parts, strip(lastp)); }
                        StringBuilder.destroy(addr(sbt));

                        StringBuilder.append(addr(sb), base_ident);
                        if len(parts) == 1 {
                            StringBuilder.append_c(addr(sb), "__T_");
                            StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(parts.data[0])));
                        } elif len(parts) == 2 {
                            StringBuilder.append_c(addr(sb), "__T2_");
                            StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(parts.data[1])));
                            StringBuilder.append_c(addr(sb), "__T_");
                            StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(parts.data[0])));
                        } else {
                            mut first_p: bool = true;
                            mut ip: i32 = 0;
                            loop {
                                if ip >= len(parts) { break; }
                                if !first_p { StringBuilder.append_c(addr(sb), "__"); }
                                StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(parts.data[ip])));
                                first_p = false;
                                ip++;
                            }
                        }

                        i = k;
                        continue;
                    }
                }
            }
            
            if k < expr_len and get_char(expr, k) == '.' {
                mut m: i32 = k + 1;
                loop {
                    if m >= expr_len { break; }
                    val wc: char = get_char(expr, m);
                    if wc != ' ' and wc != '\t' and wc != '\n' { break; }
                    m++;
                }
                if m < expr_len and is_ident_start(get_char(expr, m)) {
                    val method_start: i32 = m;
                    loop {
                        if m >= expr_len { break; }
                        val mc: char = get_char(expr, m);
                        if !is_token_char(mc) { break; }
                        m++;
                    }
                    val method_name: string = substring_se(expr, method_start, m);
                    
                    loop {
                        if m >= expr_len { break; }
                        val wc2: char = get_char(expr, m);
                        if wc2 != ' ' and wc2 != '\t' and wc2 != '\n' { break; }
                        m++;
                    }
                    
                    if m < expr_len and get_char(expr, m) == '[' {
                        m++;
                        val type_start2: i32 = m;
                        mut bd2: i32 = 1;
                        loop {
                            if m >= expr_len or bd2 == 0 { break; }
                            val tc2: char = get_char(expr, m);
                            if tc2 == '[' { bd2++; }
                            elif tc2 == ']' { bd2--; }
                            if bd2 > 0 { m++; }
                        }
                        val type_arg2: string = strip(substring_se(expr, type_start2, m));
                        if m < expr_len and get_char(expr, m) == ']' {
                            m++;
                            loop {
                                if m >= expr_len { break; }
                                val wc3: char = get_char(expr, m);
                                if wc3 != ' ' and wc3 != '\t' and wc3 != '\n' { break; }
                                m++;
                            }
                            if m < expr_len and get_char(expr, m) == '(' {
                                StringBuilder.append(addr(sb), ident);
                                StringBuilder.append_c(addr(sb), "__");
                                StringBuilder.append(addr(sb), method_name);
                                StringBuilder.append_c(addr(sb), "__T_");
                                StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(type_arg2)));
                                i = m;
                                continue;
                            }
                        }
                    } elif m < expr_len and get_char(expr, m) == '(' {
                        mut sb_full_name: StringBuilder = StringBuilder.init(str_len(ident) + str_len(method_name) + 2);
                        StringBuilder.append(addr(sb_full_name), ident);
                        StringBuilder.append_c(addr(sb_full_name), "__");
                        StringBuilder.append(addr(sb_full_name), method_name);
                        val full_name: string = StringBuilder.to_string(addr(sb_full_name));
                        StringBuilder.destroy(addr(sb_full_name));
                        
                        if StringBoolMap.contains(addr(g_generic_functions), full_name) {
                            mut arg_start2: i32 = m + 1;
                            loop {
                                if arg_start2 >= expr_len { break; }
                                val wc4: char = get_char(expr, arg_start2);
                                if wc4 != ' ' and wc4 != '\t' and wc4 != '\n' { break; }
                                arg_start2++;
                            }
                            
                            mut inferred_type2: string = str("");
                            if arg_start2 < expr_len {
                                val first_ch2: char = get_char(expr, arg_start2);
                                if first_ch2 == '"' {
                                    inferred_type2 = str("ref char");
                                } elif first_ch2 == '-' or (first_ch2 >= '0' and first_ch2 <= '9') {
                                    mut has_decimal2: bool = false;
                                    mut scan_pos2: i32 = arg_start2;
                                    loop {
                                        if scan_pos2 >= expr_len { break; }
                                        val sc2: char = get_char(expr, scan_pos2);
                                        if sc2 == '.' { has_decimal2 = true; break; }
                                        if sc2 == ',' or sc2 == ')' or sc2 == ' ' or sc2 == '\t' { break; }
                                        scan_pos2++;
                                    }
                                    if has_decimal2 { inferred_type2 = str("f32"); }
                                    else { inferred_type2 = str("i32"); }
                                } elif is_ident_start(first_ch2) {
                                    mut var_end2: i32 = arg_start2;
                                    loop {
                                        if var_end2 >= expr_len { break; }
                                        val vc2: char = get_char(expr, var_end2);
                                        if !is_token_char(vc2) { break; }
                                        var_end2++;
                                    }
                                    val arg_var_name2: string = substring_se(expr, arg_start2, var_end2);
                                    if StringStringMap.contains(addr(g_var_types), arg_var_name2) {
                                        inferred_type2 = StringStringMap.get(addr(g_var_types), arg_var_name2);
                                    }
