            mut regular_children: list(ref ASTNode);
            mut defer_children: list(ref ASTNode);

            mut i_for: i32 = 0;
            loop {
                if i_for >= len(deref(children)) {
                    break;
                }
                val child: ref ASTNode = addr(children.data[i_for]);
                if equals_c(child.node_type, "Defer") {
                    append(defer_children, child);
                } else {
                    append(regular_children, child);
                }
                i_for = i_for + 1;
            }

            mut reg_idx: i32 = 0;
            loop {
                if reg_idx >= len(regular_children) {
