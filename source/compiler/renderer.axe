                        val lastp2: string = StringBuilder.to_string(addr(sbp));
                        if str_len(strip(lastp2)) > 0 { append(arg_parts, strip(lastp2)); }
                        StringBuilder.destroy(addr(sbp));

                        mut type1: string = str("");
