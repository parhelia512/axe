                         if !is_method_part and (StringStringMap.contains(addr(g_function_param_types), ident) or 
                            StringStringMap.contains(addr(g_function_prefixes), ident)) {
                             
                            val v_start: i32 = k;
                            mut v_paren_depth: i32 = 0;
                            mut v_in_str: bool = false;
                            mut v_in_char: bool = false;
                            mut v_end: i32 = -1;
                            mut vi: i32 = v_start;

                            loop {
