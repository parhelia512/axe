                        }
                    }

                    if str_len(var_name) > 0 and str_len(type_name) > 0 {
                        mut resolved_type_name: string = type_name;
                        if g_current_type_mapping != nil and StringStringMap.contains(g_current_type_mapping, type_name) {
                            resolved_type_name = StringStringMap.get(g_current_type_mapping, type_name);
                        }

                        mut arena_decl: Arena = Arena.create(2560);
                        StringStringMap.set(addr(g_var_types), addr(arena_decl), var_name, resolved_type_name);

                        if has_prefix(resolved_type_name, str("list(")) {
                            mut paren_pos: i32 = 5;
                            mut depth: i32 = 1;
                            loop {
                                if paren_pos >= str_len(resolved_type_name) {
                                    break;
                                }
                                val ch: char = get_char(resolved_type_name, paren_pos);
                                if ch == '(' {
                                    depth++;
                                } elif ch == ')' {
                                    depth--;
                                    if depth == 0 {
                                        break;
                                    }
                                }
                                paren_pos++;
                            }
                            if depth == 0 {
                                val element_type: string = strip(substring_se(resolved_type_name, 5, paren_pos));
                                mut list_arena: Arena = Arena.create(2560);
                                StringStringMap.set(addr(g_list_of_types), addr(list_arena), var_name, element_type);
                            }
                        } elif has_suffix(resolved_type_name, str("[999]")) {
                            val element_type: string = strip(substring_se(resolved_type_name, 0, str_len(resolved_type_name) - 5));
                            mut list_arena: Arena = Arena.create(2560);
                            StringStringMap.set(addr(g_list_of_types), addr(list_arena), var_name, element_type);
                        }
                    }

                    mut mapped_type: string = map_axe_type_to_c(type_name);
                    if str_len(type_name) > 0 and equals_c(type_name, "auto") {
                        mapped_type = str("__auto_type");
                    }
                    mut array_suffix: string = str("");
                    val br: i32 = find_char_from(type_name, '[', cast[usize](0));

                    if br >= 0 {
                        val suffix: string = substring_se(type_name, br, cast[i32](str_len(type_name)));
                        if !equals_c(suffix, "[999]") {
                            val base_raw: string = strip(substring_se(type_name, 0, br));
                            if str_len(base_raw) > 0 {
                                mapped_type = map_axe_type_to_c(base_raw);
                                array_suffix = suffix;
                            }
                        }
                    }

                    StringBuilder.append_c(addr(sb_test), "    ");
                    if !is_mutable {
                        StringBuilder.append_c(addr(sb_test), "const ");
                    }
                    StringBuilder.append(addr(sb_test), mapped_type);
                    StringBuilder.append_char(addr(sb_test), ' ');
                    StringBuilder.append(addr(sb_test), var_name);
                    if str_len(array_suffix) > 0 {
                        StringBuilder.append(addr(sb_test), array_suffix);
                    }

                    mut trimmed_init: string = strip(initializer);
                    if str_len(trimmed_init) > 0 {
                        if has_prefix(trimmed_init, str("[")) and has_suffix(trimmed_init, str("]")) {
                            val elements_str: string = substring_se(trimmed_init, 1, str_len(trimmed_init) - 1);
                            val elements_trimmed: string = strip(elements_str);
                            mut element_count: i32 = 0;
                            if str_len(elements_trimmed) > 0 {
                                element_count = 1;
                                mut depth: i32 = 0;
                                mut idx_elem: i32 = 0;
                                loop {
                                    if idx_elem >= str_len(elements_trimmed) {
                                        break;
                                    }
                                    val ch_elem: char = get_char(elements_trimmed, idx_elem);
                                    if ch_elem == '(' or ch_elem == '[' or ch_elem == '{' {
                                        depth++;
                                    } elif ch_elem == ')' or ch_elem == ']' or ch_elem == '}' {
                                        depth--;
                                    } elif ch_elem == ',' and depth == 0 {
                                        element_count++;
                                    }
                                    idx_elem++;
                                }
                            }
                            
                            mut elem_type_c: string = str("int32_t");

                            if has_prefix(type_name, str("list(")) {
                                mut paren_end: i32 = 5;
                                mut depth_type: i32 = 1;
                                loop {
                                    if paren_end >= str_len(type_name) {
                                        break;
                                    }
                                    val ch_t: char = get_char(type_name, paren_end);
                                    if ch_t == '(' {
                                        depth_type++;
                                    } elif ch_t == ')' {
                                        depth_type--;
                                        if depth_type == 0 {
                                            break;
                                        }
                                    }
                                    paren_end++;
                                }
                                val elem_type_axe: string = strip(substr(type_name, 5, paren_end - 5));
                                elem_type_c = map_axe_type_to_c(elem_type_axe);
                            }

                            if equals_c(elem_type_c, "int32_t") and StringStringMap.contains(addr(g_list_of_types), var_name) {
                                val element_type_recorded: string = StringStringMap.get(addr(g_list_of_types), var_name);
                                elem_type_c = map_axe_type_to_c(element_type_recorded);
                            }

                            if equals_c(elem_type_c, "int32_t") and StringStringMap.contains(addr(g_var_types), var_name) {
                                val var_type_recorded: string = StringStringMap.get(addr(g_var_types), var_name);
                                if has_prefix(var_type_recorded, str("list(")) or has_suffix(var_type_recorded, str("[999]")) {
                                    mut element_type2: string = var_type_recorded;
                                    if has_prefix(element_type2, str("list(")) {
                                        mut paren_pos2: i32 = 5;
                                        mut depth2: i32 = 1;
                                        loop {
                                            if paren_pos2 >= str_len(element_type2) {
                                                break;
                                            }
                                            val ch2: char = get_char(element_type2, paren_pos2);
                                            if ch2 == '(' {
                                                depth2++;
                                            } elif ch2 == ')' {
                                                depth2--;
                                                if depth2 == 0 {
                                                    break;
                                                }
                                            }
                                            paren_pos2 = paren_pos2 + 1;
                                        }
                                        element_type2 = strip(substr(var_type_recorded, 5, paren_pos2 - 5));
                                    } elif has_suffix(element_type2, str("[999]")) {
                                        element_type2 = strip(substring_se(var_type_recorded, 0, str_len(var_type_recorded) - 5));
                                    }
                                    elem_type_c = map_axe_type_to_c(element_type2);
                                }
                            }

                            if equals_c(elem_type_c, "int32_t") and has_prefix(mapped_type, str("__list_")) {
                                mut san: string = substr(mapped_type, 7, str_len(mapped_type) - 7);
                                if has_suffix(san, str("_t")) {
                                    san = substring_se(san, 0, str_len(san) - 2);
                                }
                                elem_type_c = san;
                            }
                            
                            val processed_elements: string = process_expression(elements_trimmed);

                            StringBuilder.append_c(addr(sb_test), " = {.data = (");
                            StringBuilder.append(addr(sb_test), elem_type_c);
                            StringBuilder.append_c(addr(sb_test), "[]){");
                            StringBuilder.append(addr(sb_test), processed_elements);
                            StringBuilder.append_c(addr(sb_test), "}, .len = ");
                            StringBuilder.append(addr(sb_test), i32_to_string(element_count));
                            StringBuilder.append_c(addr(sb_test), ", .capacity = ");
                            StringBuilder.append(addr(sb_test), i32_to_string(element_count));
                            StringBuilder.append_c(addr(sb_test), "}");
                        } else {
                            val init_norm: string = process_expression(trimmed_init);
                            StringBuilder.append_c(addr(sb_test), " = ");
                            StringBuilder.append(addr(sb_test), init_norm);
                        }
                    }

                    StringBuilder.append_c(addr(sb_test), ";\n");
                } else {
                    val stmt_code: string = generate_c(child);
                    if str_len(stmt_code) > 0 {
                        val buf: string = stmt_code;
                        mut idx: i32 = 0;
                        val buf_len: i32 = cast[i32](str_len(buf));

                        loop {
                            if idx >= buf_len {
                                break;
                            }
                            val ch: char = get_char(buf, idx);
                            if ch == '\n' {
                                StringBuilder.append_c(addr(sb_test), "\n    ");
                            } else {
                                StringBuilder.append_char(addr(sb_test), ch);
                            }
                            idx++;
                        }
                        StringBuilder.append_char(addr(sb_test), '\n');
                    }
                }

                i_test++;
            }
        }

        StringBuilder.append_c(addr(sb_test), "    printf(\"\\n\");\n    if (failed == 0) {\n        printf(\"All tests passed. (%d/%d)\\n\", passed, passed + failed);\n    } else {\n        printf(\"%d test(s) failed, %d passed\\n\", failed, passed);\n    }\n    return failed > 0 ? 1 : 0;\n}\n");

        result = StringBuilder.to_string(addr(sb_test));
        StringBuilder.destroy(addr(sb_test));
        return result;
    }
    
    if equals_c(node_type, "Assert") {
        val cond_raw: string = strip(ast.data.assert_node.condition);
        val cond_norm: string = process_expression(cond_raw);
        val msg: string = ast.data.assert_node.message;

        mut sb_assert: StringBuilder = StringBuilder.init(512);
        StringBuilder.append_c(addr(sb_assert), "if (");
        StringBuilder.append(addr(sb_assert), cond_norm);
        StringBuilder.append_c(addr(sb_assert), ") {\n    printf(\"PASSED: ");
        StringBuilder.append(addr(sb_assert), msg);
        StringBuilder.append_c(addr(sb_assert), "\\n\");\n    passed++;\n} else {\n    printf(\"FAILED: ");
        StringBuilder.append(addr(sb_assert), msg);
        StringBuilder.append_c(addr(sb_assert), "\\n\");\n    failed++;\n}\n\n");
        return StringBuilder.to_string(addr(sb_assert));
    }
    
    if equals_c(node_type, "Function") {
        mut func_name: string = ast.data.function.name;
        mut return_type: string = ast.data.function.return_type;
        val is_public: bool = ast.data.function.is_public;
        val is_generic: bool = ast.data.function.is_generic;
        val old_prefix: string = g_current_module_prefix;
        val old_model: string = g_current_model;
        defer { 
            g_current_model = old_model;
            g_current_module_prefix = old_prefix;
            StringBoolMap.clear(addr(g_current_function_type_params));
        }

        g_current_module_prefix = get_prefix_from_mangled(func_name);

        // Extract model name from mangled function name if it's a method
        mut last_sep: i32 = -1;
        mut si: i32 = 0;
        val slen: i32 = cast[i32](str_len(func_name));
        loop {
            if si + 1 >= slen { break; }
            if get_char(func_name, si) == '_' and get_char(func_name, si+1) == '_' {
                last_sep = si;
            }
            si++;
        }
        if last_sep >= 0 {
            val model_candidate: string = substring_se(func_name, 0, last_sep);
            val canon: string = canonical_model_c_name(model_candidate);
            if str_len(canon) > 0 {
                g_current_model = canon;
            }
        }

        StringBoolMap.clear(addr(g_current_function_type_params));

        if is_generic {
            val when_types: ref list(string) = ast.data.function.when_branch_types;
            val when_counts: ref list(i32) = ast.data.function.when_branch_counts;
            val when_starts: ref list(i32) = ast.data.function.when_branch_starts;
            val type_params: ref list(string) = ast.data.function.type_params;

            if type_params == nil {
                g_current_model = old_model;
                g_current_module_prefix = old_prefix;
                StringBoolMap.clear(addr(g_current_function_type_params));
                return str("");
            }

            StringBoolMap.clear(addr(g_current_function_type_params));
            mut tpi: i32 = 0;
            loop {
                if tpi >= len(deref(type_params)) {
                    break;
                }
                val tp_name: string = type_params.data[tpi];
                StringBoolMap.set(addr(g_current_function_type_params), addr(g_renderer_arena), tp_name, true);
                tpi++;
            }
            
            mut collected_constraints: list(string);
            mut has_top_level_when: bool = false;
            
            if when_types != nil and len(deref(when_types)) > 0 {
                has_top_level_when = true;
                mut wti: i32 = 0;
                loop {
                    if wti >= len(deref(when_types)) { break; }
                    append(collected_constraints, when_types.data[wti]);
                    wti++;
                }
            }
            
            if !has_top_level_when {
                if ast.children != nil {
                    mut scan_stack: list(i64);
                    val root_children: ref list(ASTNode) = ast.children;
                    mut ri: i32 = 0;
                    loop {
                        if ri >= len(deref(root_children)) { break; }
                        append(scan_stack, cast[i64](addr(root_children.data[ri])));
                        ri++;
                    }
                    
                    loop {
                        if len(scan_stack) == 0 { break; }
                        val scan_node: ref ASTNode = cast[ref ASTNode](scan_stack.data[len(scan_stack) - 1]);
                        scan_stack.len = scan_stack.len - 1;
                        
                        if equals_c(scan_node.node_type, "WhenBranch") {
                            val tp_list: ref list(string) = scan_node.data.when_branch.type_params;
                            val ct_list: ref list(string) = scan_node.data.when_branch.concrete_types;
                            if tp_list != nil and ct_list != nil {
                                mut constraint_str: string = str("");
                                mut cti: i32 = 0;
                                loop {
                                    if cti >= len(deref(tp_list)) { break; }
                                    if cti > 0 {
                                        constraint_str = concat(constraint_str, str(","));
                                    }
                                    constraint_str = concat(constraint_str, tp_list.data[cti]);
                                    constraint_str = concat(constraint_str, str(":"));
                                    constraint_str = concat(constraint_str, ct_list.data[cti]);
                                    cti++;
                                }
                                mut found: bool = false;
                                mut fi: i32 = 0;
                                loop {
                                    if fi >= len(collected_constraints) { break; }
                                    if equals_c(collected_constraints.data[fi], constraint_str.data) {
                                        found = true;
                                        break;
                                    }
                                    fi++;
                                }
                                if !found {
                                    append(collected_constraints, constraint_str);
                                }
                            }
                        }
                        
                        if scan_node.children != nil {
                            val sub_children: ref list(ASTNode) = scan_node.children;
                            mut sci: i32 = 0;
                            loop {
                                if sci >= len(deref(sub_children)) { break; }
                                append(scan_stack, cast[i64](addr(sub_children.data[sci])));
                                sci++;
                            }
                        }
                    }
                }
            }
            
            // Add on-demand specializations from call sites
            if StringStringMap.contains(addr(g_needed_specializations), func_name) {
                val needed_raw: string = StringStringMap.get(addr(g_needed_specializations), func_name);
                mut start: i32 = 0;
                val n_needed: i32 = cast[i32](str_len(needed_raw));
                loop {
                    if start >= n_needed { break; }
                    mut end: i32 = find_char_from(needed_raw, ';', cast[usize](start));
                    if end < 0 { end = n_needed; }
                    val one_needed: string = substring_se(needed_raw, start, end);
                    if str_len(one_needed) > 0 {
                        mut found_n: bool = false;
                        mut ni: i32 = 0;
                        loop {
                            if ni >= len(collected_constraints) { break; }
                            if equals_c(collected_constraints.data[ni], one_needed.data) {
                                found_n = true;
                                break;
                            }
                            ni++;
                        }
                        if !found_n {
                            append(collected_constraints, one_needed);
                        }
                    }
                    start = end + 1;
                }
            }
            
            if !has_top_level_when and len(collected_constraints) == 0 {
                g_current_model = old_model;
                g_current_module_prefix = old_prefix;
                StringBoolMap.clear(addr(g_current_function_type_params));
                return str("");
            }
            
            mut sb_all_funcs: StringBuilder = StringBuilder.init(32768);
            mut branch_idx: i32 = 0;
            mut constraint_list_len: i32 = 0;

            if has_top_level_when {
                constraint_list_len = len(collected_constraints);
            } else {
                constraint_list_len = len(collected_constraints);
            }
            
            
            loop {
                if branch_idx >= constraint_list_len {
                    break;
                }
                
                mut constraint_str: string = collected_constraints.data[branch_idx];
                val child_count: i32 = 0;
                
                mut sb_spec_name: StringBuilder = StringBuilder.init(256);
                
                mut base_name_for_spec: string = func_name;
                if StringStringMap.contains(addr(g_function_prefixes), func_name) {
                    base_name_for_spec = StringStringMap.get(addr(g_function_prefixes), func_name);
                }
                StringBuilder.append(addr(sb_spec_name), base_name_for_spec);
                mut constraint_idx: i32 = 0;
                mut constraint_start: i32 = 0;
                val constraint_len: i32 = cast[i32](str_len(constraint_str));
                
                loop {
                    if constraint_idx > constraint_len {
                        break;
                    }
                    
                    mut is_sep: bool = false;
                    if constraint_idx == constraint_len {
                        is_sep = true;
                    } elif get_char(constraint_str, constraint_idx) == ',' {
                        is_sep = true;
                    }
                    
                    if is_sep {
                        val one_constraint: string = substring_se(constraint_str, constraint_start, constraint_idx);
                        val colon_pos: i32 = find_char_from(one_constraint, ':', cast[usize](0));
                        if colon_pos > 0 {
                            val param_name: string = substring_se(one_constraint, 0, colon_pos);
                            val concrete_tp_raw: string = substring_se(one_constraint, colon_pos + 1, cast[i32](str_len(one_constraint)));
                            val concrete_tp: string = map_axe_type_to_c(concrete_tp_raw);

                            StringBuilder.append_c(addr(sb_spec_name), "__");
                            StringBuilder.append(addr(sb_spec_name), param_name);
                            StringBuilder.append_c(addr(sb_spec_name), "_");
                            StringBuilder.append(addr(sb_spec_name), sanitize_c_identifier(concrete_tp));
                        }
                        constraint_start = constraint_idx + 1;
                    }
                    
                    constraint_idx++;
                }

                val spec_func_name: string = StringBuilder.to_string(addr(sb_spec_name));

                StringBuilder.destroy(addr(sb_spec_name));
                
                if StringBoolMap.contains(addr(g_generated_functions), spec_func_name) {
                    branch_idx++;
                    continue;
                }
                
                mut gen_arena: Arena = Arena.create(1024);
                StringBoolMap.set(addr(g_generated_functions), addr(gen_arena), spec_func_name, true);

                mut type_mapping: StringStringMap = deref(StringStringMap.create(addr(gen_arena), 16));
                mut tm_idx: i32 = 0;
                mut tm_start: i32 = 0;
                val tm_len: i32 = cast[i32](str_len(constraint_str));
                
                loop {
                    if tm_idx > tm_len {
                        break;
                    }
                    
                    mut is_tm_sep: bool = false;
                    if tm_idx == tm_len {
                        is_tm_sep = true;
                    } elif get_char(constraint_str, tm_idx) == ',' {
                        is_tm_sep = true;
                    }
                    
                    if is_tm_sep {
                        val tm_constraint: string = substring_se(constraint_str, tm_start, tm_idx);
                        val tm_colon: i32 = find_char_from(tm_constraint, ':', cast[usize](0));
                        if tm_colon > 0 {
                            val tm_param: string = substring_se(tm_constraint, 0, tm_colon);
                            val tm_concrete_raw: string = substring_se(tm_constraint, tm_colon + 1, cast[i32](str_len(tm_constraint)));
                            val tm_concrete: string = map_axe_type_to_c(tm_concrete_raw);
                            StringStringMap.set(addr(type_mapping), addr(gen_arena), tm_param, tm_concrete);
                        }
                        tm_start = tm_idx + 1;
                    }
                    
                    tm_idx++;
                }
                
                mut spec_return_type: string = return_type;
                
                if str_len(spec_return_type) == 0 {
                    spec_return_type = str("void");
                }

                if StringStringMap.contains(addr(type_mapping), return_type) {
                    spec_return_type = StringStringMap.get(addr(type_mapping), return_type);
                }
                
                val saved_type_mapping: ref StringStringMap = g_current_type_mapping;
                g_current_type_mapping = addr(type_mapping);

                val mapped_ret: string = map_axe_type_to_c(spec_return_type);
                mut sb_func: StringBuilder = StringBuilder.init(4096);
                
                StringBuilder.append(addr(sb_func), emit_line_directive(ast.line));
                
                val func_tags: ref list(string) = ast.data.function.tags;
                if func_tags != nil {
                    mut tag_idx: i32 = 0;
                    loop {
                        if tag_idx >= len(deref(func_tags)) {
                            break;
                        }
                        val tag: string = func_tags.data[tag_idx];
                        if equals_c(tag, "inline") {
                            StringBuilder.append_c(addr(sb_func), "static inline ");
                        } elif equals_c(tag, "noinline") {
                            StringBuilder.append_c(addr(sb_func), "__attribute__((noinline)) ");
                        } elif equals_c(tag, "cold") {
                            StringBuilder.append_c(addr(sb_func), "__attribute__((cold)) ");
                        } elif equals_c(tag, "hot") {
                            StringBuilder.append_c(addr(sb_func), "__attribute__((hot)) ");
                        }
                        tag_idx++;
                    }
                }
                
                StringBuilder.append(addr(sb_func), mapped_ret);
                StringBuilder.append_char(addr(sb_func), ' ');
                StringBuilder.append(addr(sb_func), spec_func_name);
                StringBuilder.append_char(addr(sb_func), '(');
                
                val params: ref list(string) = ast.data.function.params;
                if params != nil {
                    mut param_idx: i32 = 0;
                    loop {
                        if param_idx >= len(deref(params)) {
                            break;
                        }
                        
                        if param_idx > 0 {
                            StringBuilder.append_c(addr(sb_func), ", ");
                        }
                        
                        val param: string = params.data[param_idx];
                        val colon_pos: i32 = find_char_from(param, ':', cast[usize](0));
                        
                        if colon_pos >= 0 {
                            val param_name: string = strip(substring_se(param, 0, colon_pos));
                            mut param_type: string = strip(substr(param, colon_pos + 1, str_len(param) - colon_pos - 1));
                            
                            // Check if param_type matches any type parameter in our mapping
                            if StringStringMap.contains(addr(type_mapping), param_type) {
                                param_type = StringStringMap.get(addr(type_mapping), param_type);
                            }
                            
                            val processed_type: string = process_parameter_type(param_type);
                            StringBuilder.append(addr(sb_func), processed_type);
                            StringBuilder.append_char(addr(sb_func), ' ');
                            StringBuilder.append(addr(sb_func), param_name);
                            StringStringMap.set(addr(g_var_types), addr(gen_arena), param_name, param_type);
                        } else {
                            StringBuilder.append(addr(sb_func), param);
                        }
                        
                        param_idx++;
                    }
                }
                
                StringBuilder.append_c(addr(sb_func), ") {\n");
                
                if ast.children != nil {
                    val children: ref list(ASTNode) = ast.children;
                    mut ci: i32 = 0;

                    if has_top_level_when {
                        mut next_check_branch: i32 = 0;

                        loop {
                            if ci >= len(deref(children)) {
                                break;
                            }

                            mut is_branch_start: bool = false;
                            if when_starts != nil and next_check_branch < len(deref(when_starts)) {
                                 if ci == when_starts.data[next_check_branch] {
                                     is_branch_start = true;
                                 }
                            }

                            if is_branch_start {
                                val branch_constraint: string = when_types.data[next_check_branch];
                                val b_count: i32 = when_counts.data[next_check_branch];
                                val is_match: bool = constraints_match(constraint_str, branch_constraint);
                                
                                if is_match {
                                    mut bi: i32 = 0;
                                    loop {
                                         if bi >= b_count { break; }
                                         val child_idx: i32 = ci + bi;
                                         if child_idx < len(deref(children)) {
                                             val child_code: string = generate_c(addr(children.data[child_idx]));
                                             StringBuilder.append(addr(sb_func), child_code);
                                         }
                                         bi++;
                                    }
                                }
                                ci = ci + b_count;
                                next_check_branch++;
                                continue;
                            }

                            val common_code: string = generate_c(addr(children.data[ci]));
                            StringBuilder.append(addr(sb_func), common_code);
                            ci++;
                        }
                    } else {
                        mut regular_children: list(ref ASTNode);
                        mut defer_children: list(ref ASTNode);

                        mut child_idx: i32 = 0;
                        loop {
                            if child_idx >= len(deref(children)) {
                                break;
                            }
                            val child: ref ASTNode = addr(children.data[child_idx]);
                            if equals_c(child.node_type, "Defer") {
                                append(defer_children, child);
                            } else {
                                append(regular_children, child);
                            }
                            child_idx++;
                        }

                        mut reg_idx: i32 = 0;
                        loop {
                            if reg_idx >= len(regular_children) {
                                break;
                            }
                            val child: ref ASTNode = regular_children.data[reg_idx];
                            val child_code: string = generate_c(child);
                            StringBuilder.append(addr(sb_func), child_code);
                            reg_idx++;
                        }

                        mut defer_idx: i32 = 0;
                        loop {
                            if defer_idx >= len(defer_children) {
                                break;
                            }
                            val defer_node: ref ASTNode = defer_children.data[defer_idx];
                            val body: ref list(ASTNode) = defer_node.data.defer_node.body;
                            if body != nil {
                                mut i_defer: i32 = 0;
                                loop {
                                    if i_defer >= len(deref(body)) {
                                        break;
                                    }
                                    val defer_child_code: string = generate_c(addr(body.data[i_defer]));
                                    StringBuilder.append(addr(sb_func), defer_child_code);
                                    i_defer++;
                                }
                            }
                            defer_idx++;
                        }
                    }

                    g_current_type_mapping = saved_type_mapping;
                }
                
                StringBuilder.append_c(addr(sb_func), "}\n\n");
                
                val func_code: string = StringBuilder.to_string(addr(sb_func));
                StringBuilder.destroy(addr(sb_func));
                StringBuilder.append(addr(sb_all_funcs), func_code);
                
                branch_idx = branch_idx + 1;
            }
            
            val all_funcs_result: string = StringBuilder.to_string(addr(sb_all_funcs));
            g_current_model = old_model;
            g_current_module_prefix = old_prefix;
            StringBoolMap.clear(addr(g_current_function_type_params));
            return all_funcs_result;
        }
        
        if !equals_c(func_name, "main") {
            mut sep2: i32 = -1;
            mut i2: i32 = 0;
            val n2: i32 = cast[i32](str_len(func_name));

            loop {
                if i2 + 1 >= n2 {
                    break;
                }
                val c1b: char = get_char(func_name, i2);
                val c2b: char = get_char(func_name, i2 + 1);
                if c1b == '_' and c2b == '_' {
                    sep2 = i2;
                }
                i2++;
            }

            if sep2 >= 0 and sep2 + 2 < n2 {
                val model_part2: string = substring_se(func_name, 0, sep2);
                val method_part2: string = substring_se(func_name, sep2 + 2, n2);
                mut has_inner_sep2: bool = false;
                mut j2: i32 = 0;
                val mlen2: i32 = cast[i32](str_len(model_part2));
                loop {
                    if j2 + 1 >= mlen2 {
                        break;
                    }
                    val mc1b: char = get_char(model_part2, j2);
                    val mc2b: char = get_char(model_part2, j2 + 1);
                    if mc1b == '_' and mc2b == '_' {
                        has_inner_sep2 = true;
                        break;
                    }
                    j2++;
                }

                if !has_inner_sep2 {
                    val canon_model2: string = canonical_model_c_name(model_part2);
                    if str_len(canon_model2) > 0 {
                        mut sb_fname: StringBuilder = StringBuilder.init(str_len(canon_model2) + str_len(method_part2) + 2);
                        StringBuilder.append(addr(sb_fname), canon_model2);
                        StringBuilder.append_c(addr(sb_fname), "__");
                        StringBuilder.append(addr(sb_fname), method_part2);
                        func_name = StringBuilder.to_string(addr(sb_fname));
                        StringBuilder.destroy(addr(sb_fname));
                    }
                }
            }

            if StringStringMap.contains(addr(g_function_prefixes), func_name) {
                func_name = StringStringMap.get(addr(g_function_prefixes), func_name);
            }
        }
        
        if StringBoolMap.contains(addr(g_generated_functions), func_name) {
            g_current_model = old_model;
            g_current_module_prefix = old_prefix;
            StringBoolMap.clear(addr(g_current_function_type_params));
            return str("");
        }
        
        mut func_arena: Arena = Arena.create(1024);

        StringBoolMap.set(addr(g_generated_functions), addr(func_arena), func_name, true);
        StringStringMap.clear(addr(g_var_types));
        
        validate_type_imported(return_type);
        
        val fn_params: ref list(string) = ast.data.function.params;
        if fn_params != nil {
            mut pi: i32 = 0;
            loop {
                if pi >= len(deref(fn_params)) {
                    break;
                }
                val p: string = fn_params.data[pi];
                val cp: i32 = find_char_from(p, ':', cast[usize](0));
                if cp >= 0 {
                    val pname: string = strip(substring_se(p, 0, cp));
                    val ptype: string = strip(substr(p, cp + 1, cast[i32](str_len(p) - cp - 1)));
                    if str_len(pname) > 0 and str_len(ptype) > 0 {
                        validate_type_imported(ptype);
                        StringStringMap.set(addr(g_var_types), addr(func_arena), pname, ptype);
                    }
                }
                pi = pi + 1;
            }
        }
        
        if equals_c(func_name, "main") {
            mut sb_main: StringBuilder = StringBuilder.init(4096);
            StringBuilder.append_c(addr(sb_main), "int main(int argc, char** argv) {\n__axe_argc = argc;\n__axe_argv = argv;\n");

            platform windows {
                StringBuilder.append_c(addr(sb_main), "SetConsoleOutputCP(65001);\n");
            }

            if !::is_release_build {
                StringBuilder.append(addr(sb_main), generate_stack_trace_setup());
            }
            
            if ast.children != nil {
                val children: ref list(ASTNode) = ast.children;
                mut i: i32 = 0;
                loop {
                    if i >= len(deref(children)) {
                        break;
                    }
                    val child_code: string = generate_c(addr(children.data[i]));
                    StringBuilder.append(addr(sb_main), child_code);
                    i++;
                }
            }
            
            StringBuilder.append_c(addr(sb_main), "return 0;\n}\n\n");
            return StringBuilder.to_string(addr(sb_main));
        }
        
        if str_len(return_type) == 0 {
            return_type = str("void");
        }
        
        val mapped_return_type: string = map_axe_type_to_c(return_type);
        mut sb_func: StringBuilder = StringBuilder.init(4096);
        
        StringBuilder.append(addr(sb_func), emit_line_directive(ast.line));
        
        val func_tags: ref list(string) = ast.data.function.tags;
        if func_tags != nil {
            mut tag_idx: i32 = 0;
            loop {
                if tag_idx >= len(deref(func_tags)) {
                    break;
                }
                val tag: string = func_tags.data[tag_idx];
                if equals_c(tag, "inline") {
                    StringBuilder.append_c(addr(sb_func), "static inline ");
                } elif equals_c(tag, "noinline") {
                    StringBuilder.append_c(addr(sb_func), "__attribute__((noinline)) ");
                } elif equals_c(tag, "cold") {
                    StringBuilder.append_c(addr(sb_func), "__attribute__((cold)) ");
                } elif equals_c(tag, "hot") {
                    StringBuilder.append_c(addr(sb_func), "__attribute__((hot)) ");
                }
                tag_idx = tag_idx + 1;
            }
        }
        
        StringBuilder.append(addr(sb_func), mapped_return_type);
        StringBuilder.append_char(addr(sb_func), ' ');
        StringBuilder.append(addr(sb_func), func_name);
        StringBuilder.append_char(addr(sb_func), '(');
        
        val params: ref list(string) = ast.data.function.params;
        if params != nil {
            mut param_idx: i32 = 0;
            loop {
                if param_idx >= len(deref(params)) {
                    break;
                }
                
                if param_idx > 0 {
                    StringBuilder.append_c(addr(sb_func), ", ");
                }
                
                val param: string = params.data[param_idx];
                val colon_pos: i32 = find_char_from(param, ':', cast[usize](0));

                if colon_pos >= 0 {
                    val param_name: string = strip(substring_se(param, 0, colon_pos));
                    val param_type: string = strip(substr(param, colon_pos + 1, cast[i32](str_len(param) - colon_pos - 1)));
                    val processed_type: string = process_parameter_type(param_type);
                    
                    StringBuilder.append(addr(sb_func), processed_type);
                    StringBuilder.append_char(addr(sb_func), ' ');
                    StringBuilder.append(addr(sb_func), param_name);
                } else {
                    StringBuilder.append(addr(sb_func), param);
                }
                
                param_idx++;
            }
        }
        
        StringBuilder.append_c(addr(sb_func), ") {\n");
        
        if ast.children != nil {
            val children: ref list(ASTNode) = ast.children;
            mut i: i32 = 0;
            loop {
                if i >= len(deref(children)) {
                    break;
                }
                val child_code: string = generate_c(addr(children.data[i]));
                StringBuilder.append(addr(sb_func), child_code);
                i++;
            }
        }
        
        StringBuilder.append_c(addr(sb_func), "}\n\n");
        result = StringBuilder.to_string(addr(sb_func));
        StringBuilder.destroy(addr(sb_func));
        
        g_current_model = old_model;
        g_current_module_prefix = old_prefix;
        StringBoolMap.clear(addr(g_current_function_type_params));
        return result;
    }
    
    if equals_c(node_type, "Return") {
        mut sb_ret: StringBuilder = StringBuilder.init(512);
        StringBuilder.append_c(addr(sb_ret), "return ");
        mut expr: string = ast.data.return_node.expression;
        debug_print_raw "\n[DEBUG] RENDERER raw return expr:";
        debug_print_str expr;

        mut trimmed_ret: string = strip(expr);

        val paren_pos_ret: i32 = find_char_from(trimmed_ret, '(', cast[usize](0));
        if paren_pos_ret > 0 {
            val call_name_ret: string = strip(substring_se(trimmed_ret, 0, paren_pos_ret));
            if StringStringMap.contains(addr(g_function_prefixes), call_name_ret) {
                val full_name_ret: string = StringStringMap.get(addr(g_function_prefixes), call_name_ret);
                val rest_ret: string = substring_se(trimmed_ret, paren_pos_ret, cast[i32](str_len(trimmed_ret)));
                mut sb_tmp: StringBuilder = StringBuilder.init(str_len(full_name_ret) + str_len(rest_ret));
                StringBuilder.append(addr(sb_tmp), full_name_ret);
                StringBuilder.append(addr(sb_tmp), rest_ret);
                trimmed_ret = StringBuilder.to_string(addr(sb_tmp));
                StringBuilder.destroy(addr(sb_tmp));
                expr = trimmed_ret;
            }
        }

        mut processed: string = str("");

        val brace_pos_ret: i32 = find_char_from(trimmed_ret, '{', cast[usize](0));
        if brace_pos_ret > 0 and get_char(trimmed_ret, cast[i32](str_len(trimmed_ret) - 1)) == '}' {
            val prefix_ret: string = strip(substring_se(trimmed_ret, 0, brace_pos_ret));
            val inner_ret: string = substring_se(trimmed_ret, brace_pos_ret + 1, cast[i32](str_len(trimmed_ret) - 1));

            if str_len(prefix_ret) > 0 {
                val canon_ret: string = canonical_model_c_name(prefix_ret);
                mut c_type_ret: string = canon_ret;
                if str_len(c_type_ret) == 0 {
                    c_type_ret = map_axe_type_to_c(prefix_ret);
                }

                if str_len(c_type_ret) > 0 {
                    mut sb_init_ret: StringBuilder = StringBuilder.init(512);
                    StringBuilder.append_c(addr(sb_init_ret), "(");
                    StringBuilder.append(addr(sb_init_ret), c_type_ret);
                    StringBuilder.append_c(addr(sb_init_ret), "){");

                    mut sb_current_ret: StringBuilder = StringBuilder.init(str_len(inner_ret) + 16);
                    mut depth_ret: i32 = 0;
                    mut first_field_ret: bool = true;
                    mut i_ret: i32 = 0;
                    val inner_ret_len: i32 = cast[i32](str_len(inner_ret));

                    loop {
                        if i_ret >= inner_ret_len {
                            break;
                        }
                        val ch_ret: char = get_char(inner_ret, i_ret);
                        if ch_ret == '(' {
                            depth_ret = depth_ret + 1;
                            StringBuilder.append_char(addr(sb_current_ret), ch_ret);
                        } elif ch_ret == ')' {
                            if depth_ret > 0 {
                                depth_ret = depth_ret - 1;
                            }
                            StringBuilder.append_char(addr(sb_current_ret), ch_ret);
                        } elif ch_ret == ',' and depth_ret == 0 {
                            mut part_ret: string = strip(StringBuilder.to_string(addr(sb_current_ret)));
                            if str_len(part_ret) > 0 {
                                val colon_pos_ret2: i32 = find_char_from(part_ret, ':', cast[usize](0));
                                if colon_pos_ret2 >= 0 {
                                    val field_name_ret: string = strip(substring_se(part_ret, 0, colon_pos_ret2));
                                    val value_expr_ret: string = strip(substr(part_ret, colon_pos_ret2 + 1, cast[i32](str_len(part_ret) - colon_pos_ret2 - 1)));
                                    if str_len(field_name_ret) > 0 and str_len(value_expr_ret) > 0 {
                                        if !first_field_ret {
                                            StringBuilder.append_c(addr(sb_init_ret), ", ");
                                        }
                                        StringBuilder.append_c(addr(sb_init_ret), ".");
                                        StringBuilder.append(addr(sb_init_ret), field_name_ret);
                                        StringBuilder.append_c(addr(sb_init_ret), " = ");
                                        val processed_val_ret: string = process_expression(value_expr_ret);
                                        StringBuilder.append(addr(sb_init_ret), processed_val_ret);
                                        first_field_ret = false;
                                    }
                                }
                            }
                            StringBuilder.clear(addr(sb_current_ret));
                        } else {
                            StringBuilder.append_char(addr(sb_current_ret), ch_ret);
                        }
                        i_ret = i_ret + 1;
                    }

                    mut part_last_ret: string = strip(StringBuilder.to_string(addr(sb_current_ret)));
                    StringBuilder.destroy(addr(sb_current_ret));
                    if str_len(part_last_ret) > 0 {
                        val colon_pos_last_ret: i32 = find_char_from(part_last_ret, ':', cast[usize](0));
                        if colon_pos_last_ret >= 0 {
                            val field_name_last_ret: string = strip(substring_se(part_last_ret, 0, colon_pos_last_ret));
                            val value_expr_last_ret: string = strip(substr(part_last_ret, colon_pos_last_ret + 1, cast[i32](str_len(part_last_ret) - colon_pos_last_ret - 1)));
                            if str_len(field_name_last_ret) > 0 and str_len(value_expr_last_ret) > 0 {
                                if !first_field_ret {
                                    StringBuilder.append_c(addr(sb_init_ret), ", ");
                                }
                                StringBuilder.append_c(addr(sb_init_ret), ".");
                                StringBuilder.append(addr(sb_init_ret), field_name_last_ret);
                                StringBuilder.append_c(addr(sb_init_ret), " = ");
                                val processed_last_ret: string = process_expression(value_expr_last_ret);
                                StringBuilder.append(addr(sb_init_ret), processed_last_ret);
                                first_field_ret = false;
                            }
                        }
                    }

                    StringBuilder.append_c(addr(sb_init_ret), "}");
                    processed = StringBuilder.to_string(addr(sb_init_ret));
                    StringBuilder.destroy(addr(sb_init_ret));
                }
            }
        }

        if str_len(processed) == 0 {
            debug_print_raw "\n[DBG-CALL] process_expression from Return:";
            debug_print_str expr;
            processed = process_expression(expr);
        }
        StringBuilder.append(addr(sb_ret), processed);
        StringBuilder.append_c(addr(sb_ret), ";\n");
        result = StringBuilder.to_string(addr(sb_ret));
        StringBuilder.destroy(addr(sb_ret));
        return result;
    }

    if equals_c(node_type, "Break") {
        return str("break;\n");
    }

    if equals_c(node_type, "Continue") {
        return str("continue;\n");
    }
    
    if equals_c(node_type, "Declaration") {
        val var_name: string = ast.data.declaration.name;
        mut type_name: string = ast.data.declaration.type_name;
        val is_mutable: bool = ast.data.declaration.is_mutable;
        val initializer: string = ast.data.declaration.initializer;
        
        validate_type_imported(type_name);
        
        mut mapped_type: string = map_axe_type_to_c(type_name);

        if str_len(type_name) == 0 {
            mut trimmed_init: string = strip(initializer);
            if has_prefix(trimmed_init, str("new ")) {
                trimmed_init = substring_se(trimmed_init, 4, cast[i32](str_len(trimmed_init)));
                trimmed_init = strip(trimmed_init);

                mut end_pos: i32 = -1;
                mut i: i32 = 0;
                loop {
                    if i >= str_len(trimmed_init) {
                        break;
                    }
                    val ch: char = get_char(trimmed_init, i);
                    if ch == '(' or ch == ' ' or ch == '\t' {
                        end_pos = i;
                        break;
                    }
                    i++;
                }
                if end_pos < 0 {
                    end_pos = str_len(trimmed_init);
                }
                val inferred_name: string = strip(substring_se(trimmed_init, 0, end_pos));
                if str_len(inferred_name) > 0 {
                    type_name = inferred_name;
                    mapped_type = map_axe_type_to_c(type_name);
                }
            } else {
                val brace_pos: i32 = find_char_from(trimmed_init, '{', cast[usize](0));
                if brace_pos > 0 {
                    val inferred_name2: string = strip(substring_se(trimmed_init, 0, brace_pos));
                    if str_len(inferred_name2) > 0 {
                        type_name = inferred_name2;
                        mapped_type = map_axe_type_to_c(type_name);
                    }
                }
            }
        }

        // Infer type from initializer expression, this can be some kind of function call or whatever.
        if str_len(type_name) == 0 and str_len(initializer) > 0 {
            val inferred: string = infer_expression_type(strip(initializer));
            if str_len(inferred) > 0 {
                if equals_c(inferred, "cast_inferred_type") {
                    print(get_short_filename(g_current_source_file));
                    print(":");
                    print(i32_to_string(g_current_line));
                    print(": error: cannot infer cast target type for '");
                    print(initializer);
                    println("', add explicit type to the LHS.");
                    unsafe {
                        C.exit(1);
                    }
                } else {
                    type_name = inferred;
                    mapped_type = map_axe_type_to_c(type_name);
                }
            }

            if str_len(type_name) == 0 {
                val trimmed_init2: string = strip(initializer);
                val pidx2: i32 = find_char_from(trimmed_init2, '(', cast[usize](0));
                if pidx2 > 0 {
                    val call_name_raw: string = strip(substring_se(trimmed_init2, 0, pidx2));
                    if str_len(call_name_raw) > 0 and is_ident_start(get_char(call_name_raw, 0)) {
                        mapped_type = str("__auto_type");
                        type_name = str("auto");
                    }
                }
            }
        }

        // Record the declared Axe type for this variable so that later
        // MemberAccess nodes can determine whether the base object should
        // use '.' or '->' in generated C.
        if str_len(var_name) > 0 and str_len(type_name) > 0 {
            if str_len(initializer) > 0 {
                val trimmed_init: string = strip(initializer);
                mut is_numeric_literal: bool = false;
                if str_len(trimmed_init) > 0 {
                    val c0: char = get_char(trimmed_init, 0);
                    if c0 >= '0' and c0 <= '9' {
                        is_numeric_literal = true;
                    } elif c0 == '-' and str_len(trimmed_init) > 1 {
                        val c1: char = get_char(trimmed_init, 1);
                        if (c1 >= '0' and c1 <= '9') or c1 == '.' {
                            is_numeric_literal = true;
                        }
                    }
                }

                val inferred_init: string = infer_expression_type(trimmed_init);
                if str_len(inferred_init) > 0 and !equals_c(inferred_init, "auto") {
                    if equals_c(inferred_init, "cast_inferred_type") and str_len(type_name) > 0 {
                        // This is a cast without explicit type parameters, but we have target type from LHS
                        // It's handled by the transformation later, so we skip the type check here
                    } elif has_prefix(inferred_init, str("ref ")) {
                        if str_len(strip(type_name)) == 0 or equals_c(strip(type_name), "auto") {
                            type_name = inferred_init;
                            mapped_type = map_axe_type_to_c(type_name);
                        } elif has_prefix(strip(type_name), str("ref ")) or has_suffix(strip(type_name), str("*")) or equals_c(strip(type_name), "string") {
                            // ok
                        } else {
                            print(get_short_filename(g_current_source_file));
                            print(":");
                            print(i32_to_string(g_current_line));
                            print(": error: cannot initialize non-ref variable '");
                            print(var_name);
                            print("' with address: initializer has type '");
                            print(inferred_init);
                            println("'");
                            unsafe {
                                C.exit(1);
                            }
                        }
                    } elif equals_c(inferred_init, "ref void") {
                        if !(has_prefix(type_name, str("ref ")) or has_suffix(type_name, str("*"))) {
                            print(get_short_filename(g_current_source_file));
                            print(":");
                            print(i32_to_string(g_current_line));
                            print(": error: type mismatch in declaration '");
                            print(var_name);
                            print("': expected type '");
                            print(type_name);
                            print("' but initializer has type '");
                            print(inferred_init);
                            println("'");
                            unsafe {
                                C.exit(1);
                            }
                        }
                    } elif !is_numeric_literal and !equals_c(inferred_init, "ref void") {
                        val mapped_inferred: string = map_axe_type_to_c(inferred_init);
                        val mapped_expected: string = map_axe_type_to_c(type_name);

                        if compare(mapped_inferred, mapped_expected) != 0 {
                            print(get_short_filename(g_current_source_file));
                            print(":");
                            print(i32_to_string(g_current_line));
                            print(": error: type mismatch in declaration '");
                            print(var_name);
                            print("': expected type '");
                            print(type_name);
                            print("' but initializer has type '");
                            print(inferred_init);
                            println("'");
                            unsafe {
                                C.exit(1);
                            }
                        }
                    }
                }
            }

            mut decl_arena: Arena = Arena.create(2560);
            mut resolved_type_name: string = type_name;
            if g_current_type_mapping != nil and StringStringMap.contains(g_current_type_mapping, type_name) {
                resolved_type_name = StringStringMap.get(g_current_type_mapping, type_name);
            }
            StringStringMap.set(addr(g_var_types), addr(decl_arena), var_name, resolved_type_name);

            if has_prefix(resolved_type_name, str("list(")) {
                mut paren_pos: i32 = 5;
                mut depth: i32 = 1;
                loop {
                    if paren_pos >= str_len(resolved_type_name) {
                        break;
                    }
                    val ch: char = get_char(resolved_type_name, paren_pos);
                    if ch == '(' {
                        depth++;
                    } elif ch == ')' {
                        depth--;
                        if depth == 0 {
                            break;
                        }
                    }
                    paren_pos++;
                }
                if depth == 0 {
                    val element_type: string = strip(substring_se(resolved_type_name, 5, paren_pos));
                    mut resolved_element_type: string = element_type;
                    if g_current_type_mapping != nil and StringStringMap.contains(g_current_type_mapping, element_type) {
                        resolved_element_type = StringStringMap.get(g_current_type_mapping, element_type);
                    }
                    mut list_arena: Arena = Arena.create(2560);
                    StringStringMap.set(addr(g_list_of_types), addr(list_arena), var_name, resolved_element_type);
                }
            } elif has_suffix(resolved_type_name, str("[999]")) {
                val element_type: string = strip(substring_se(resolved_type_name, 0, str_len(resolved_type_name) - 5));
                mut resolved_element_type: string = element_type;
                if g_current_type_mapping != nil and StringStringMap.contains(g_current_type_mapping, element_type) {
                    resolved_element_type = StringStringMap.get(g_current_type_mapping, element_type);
                }
                mut list_arena: Arena = Arena.create(2560);
                StringStringMap.set(addr(g_list_of_types), addr(list_arena), var_name, resolved_element_type);
            }
        }

        val enum_model_name: string = canonical_model_c_name(type_name);
        if str_len(enum_model_name) > 0 and StringBoolMap.contains(addr(g_enum_names), enum_model_name) {
            mapped_type = enum_model_name;
        }

        mut array_suffix: string = str("");
        val br: i32 = find_char_from(type_name, '[', cast[usize](0));
        if br >= 0 {
            val suffix: string = substring_se(type_name, br, cast[i32](str_len(type_name)));
            if !equals_c(suffix, "[999]") {
                val base_raw: string = strip(substring_se(type_name, 0, br));
                if str_len(base_raw) > 0 {
                    mapped_type = map_axe_type_to_c(base_raw);
                    array_suffix = suffix;
                }
            }
        }

        mut sb_decl: StringBuilder = StringBuilder.init(256);
        val line_dir: string = emit_line_directive(ast.line);
        StringBuilder.append(addr(sb_decl), line_dir);
        
        if !is_mutable {
            StringBuilder.append_c(addr(sb_decl), "const ");
        }
        StringBuilder.append(addr(sb_decl), mapped_type);
        StringBuilder.append_char(addr(sb_decl), ' ');
        StringBuilder.append(addr(sb_decl), var_name);
        if str_len(array_suffix) > 0 {
            StringBuilder.append(addr(sb_decl), array_suffix);
        }
        
        if str_len(initializer) > 0 {
            mut trimmed: string = strip(initializer);

            if has_prefix(trimmed, str("[")) and has_suffix(trimmed, str("]")) {
                val elements_str: string = substring_se(trimmed, 1, str_len(trimmed) - 1);
                val elements_trimmed: string = strip(elements_str);
                mut element_count: i32 = 0;
                if str_len(elements_trimmed) > 0 {
                    element_count = 1;
                    mut depth: i32 = 0;
                    mut idx_elem: i32 = 0;
                    loop {
                        if idx_elem >= str_len(elements_trimmed) {
                            break;
                        }
                        val ch_elem: char = get_char(elements_trimmed, idx_elem);
                        if ch_elem == '(' or ch_elem == '[' or ch_elem == '{' {
                            depth++;
                        } elif ch_elem == ')' or ch_elem == ']' or ch_elem == '}' {
                            depth--;
                        } elif ch_elem == ',' and depth == 0 {
                            element_count++;
                        }
                        idx_elem++;
                    }
                }
                
                mut elem_type_c: string = str("int32_t");

                if has_prefix(type_name, str("list(")) {
                    mut paren_end: i32 = 5;
                    mut depth_type: i32 = 1;
                    loop {
                        if paren_end >= str_len(type_name) {
                            break;
                        }
                        val ch_t: char = get_char(type_name, paren_end);
                        if ch_t == '(' {
                            depth_type++;
                        } elif ch_t == ')' {
                            depth_type--;
                            if depth_type == 0 {
                                break;
                            }
                        }
                        paren_end++;
                    }
                    val elem_type_axe: string = strip(substr(type_name, 5, paren_end - 5));
                    elem_type_c = map_axe_type_to_c(elem_type_axe);
                }

                if equals_c(elem_type_c, "int32_t") and StringStringMap.contains(addr(g_list_of_types), var_name) {
                    val element_type_recorded: string = StringStringMap.get(addr(g_list_of_types), var_name);
                    elem_type_c = map_axe_type_to_c(element_type_recorded);
                }

                if equals_c(elem_type_c, "int32_t") and StringStringMap.contains(addr(g_var_types), var_name) {
                    val var_type_recorded: string = StringStringMap.get(addr(g_var_types), var_name);
                    if has_prefix(var_type_recorded, str("list(")) or has_suffix(var_type_recorded, str("[999]")) {
                        mut element_type2: string = var_type_recorded;
                        if has_prefix(element_type2, str("list(")) {
                            mut paren_pos2: i32 = 5;
                            mut depth2: i32 = 1;
                            loop {
                                if paren_pos2 >= str_len(element_type2) {
                                    break;
                                }
                                val ch2: char = get_char(element_type2, paren_pos2);
                                if ch2 == '(' {
                                    depth2++;
                                } elif ch2 == ')' {
                                    depth2--;
                                    if depth2 == 0 {
                                        break;
                                    }
                                }
                                paren_pos2 = paren_pos2 + 1;
                            }
                            element_type2 = strip(substr(var_type_recorded, 5, paren_pos2 - 5));
                        } elif has_suffix(element_type2, str("[999]")) {
                            element_type2 = strip(substring_se(var_type_recorded, 0, str_len(var_type_recorded) - 5));
                        }
                        elem_type_c = map_axe_type_to_c(element_type2);
                    }
                }

                if equals_c(elem_type_c, "int32_t") and has_prefix(mapped_type, str("__list_")) {
                    mut san: string = substr(mapped_type, 7, str_len(mapped_type) - 7);
                    if has_suffix(san, str("_t")) {
                        san = substring_se(san, 0, str_len(san) - 2);
                    }
                    elem_type_c = san;
                }
                
                val processed_elements: string = process_expression(elements_trimmed);

                StringBuilder.append_c(addr(sb_decl), " = {.data = (");
                StringBuilder.append(addr(sb_decl), elem_type_c);
                StringBuilder.append_c(addr(sb_decl), "[]){");
                StringBuilder.append(addr(sb_decl), processed_elements);
                StringBuilder.append_c(addr(sb_decl), "}, .len = ");
                StringBuilder.append(addr(sb_decl), i32_to_string(element_count));
                StringBuilder.append_c(addr(sb_decl), ", .capacity = ");
                StringBuilder.append(addr(sb_decl), i32_to_string(element_count));
                StringBuilder.append_c(addr(sb_decl), "}");
            } elif has_prefix(trimmed, str("new ")) {
                trimmed = substring_se(trimmed, 4, cast[i32](str_len(trimmed)));
                trimmed = strip(trimmed);

                val first_paren: i32 = find_char_from(trimmed, '(', cast[usize](0));
                if first_paren < 0 {
                    StringBuilder.append_c(addr(sb_decl), " = {0}");
                } else {
                    mut last_paren: i32 = -1;
                    mut idx: i32 = str_len(trimmed) - 1;
                    loop {
                        if idx < 0 {
                            break;
                        }
                        val ch_end: char = get_char(trimmed, idx);
                        if ch_end == ')' {
                            last_paren = idx;
                            break;
                        }
                        idx = idx - 1;
                    }

                    if last_paren <= first_paren + 1 {
                        // No arguments
                        StringBuilder.append_c(addr(sb_decl), " = {0}");
                    } else {
                        val args_str: string = substring_se(trimmed, first_paren + 1, last_paren);
                        StringBuilder.append_c(addr(sb_decl), " = {");

                        mut sb_current: StringBuilder = StringBuilder.init(str_len(args_str) + 16);
                        mut depth: i32 = 0;
                        mut first_field: bool = true;
                        mut i2: i32 = 0;
                        val args_str_len: i32 = cast[i32](str_len(args_str));

                        loop {
                            if i2 >= args_str_len {
                                break;
                            }
                            val ch: char = get_char(args_str, i2);
                            if ch == '(' {
                                depth = depth + 1;
                                StringBuilder.append_char(addr(sb_current), ch);
                            } elif ch == ')' {
                                if depth > 0 {
                                    depth = depth - 1;
                                }
                                StringBuilder.append_char(addr(sb_current), ch);
                            } elif ch == ',' and depth == 0 {
                                mut part: string = strip(StringBuilder.to_string(addr(sb_current)));
                                if str_len(part) > 0 {
                                    val colon_pos: i32 = find_char_from(part, ':', cast[usize](0));
                                    if colon_pos >= 0 {
                                        val field_name: string = strip(substring_se(part, 0, colon_pos));
                                        val value_expr: string = strip(substr(part, colon_pos + 1, cast[i32](str_len(part) - colon_pos - 1)));
                                        if str_len(field_name) > 0 and str_len(value_expr) > 0 {
                                            if !first_field {
                                                StringBuilder.append_c(addr(sb_decl), ", ");
                                            }
                                            StringBuilder.append_c(addr(sb_decl), ".");
                                            StringBuilder.append(addr(sb_decl), field_name);
                                            StringBuilder.append_c(addr(sb_decl), " = ");
                                            val converted_value: string = convert_union_member_init(value_expr);
                                            StringBuilder.append(addr(sb_decl), process_expression(converted_value));
                                            first_field = false;
                                        }
                                    }
                                }
                                StringBuilder.clear(addr(sb_current));
                            } else {
                                StringBuilder.append_char(addr(sb_current), ch);
                            }
                            i2++;
                        }

                        mut part_last: string = strip(StringBuilder.to_string(addr(sb_current)));
                        StringBuilder.destroy(addr(sb_current));
                        if str_len(part_last) > 0 {
                            val colon_pos_last: i32 = find_char_from(part_last, ':', cast[usize](0));
                            if colon_pos_last >= 0 {
                                val field_name_last: string = strip(substring_se(part_last, 0, colon_pos_last));
                                val value_expr_last: string = strip(substr(part_last, colon_pos_last + 1, cast[i32](str_len(part_last) - colon_pos_last - 1)));
                                if str_len(field_name_last) > 0 and str_len(value_expr_last) > 0 {
                                    if !first_field {
                                        StringBuilder.append_c(addr(sb_decl), ", ");
                                    }
                                    StringBuilder.append_c(addr(sb_decl), ".");
                                    StringBuilder.append(addr(sb_decl), field_name_last);
                                    StringBuilder.append_c(addr(sb_decl), " = ");
                                    val converted_value_last: string = convert_union_member_init(value_expr_last);
                                    StringBuilder.append(addr(sb_decl), process_expression(converted_value_last));
                                    first_field = false;
                                }
                            }
                        }

                        if first_field {
                            StringBuilder.append_c(addr(sb_decl), "}");
                        } else {
                            StringBuilder.append_c(addr(sb_decl), "}");
                        }
                    }
                }
            } else {
                val brace_pos: i32 = find_char_from(trimmed, '{', cast[usize](0));
                if brace_pos > 0 and get_char(trimmed, str_len(trimmed) - 1) == '}' {
                    val prefix: string = strip(substring_se(trimmed, 0, brace_pos));
                    val inner: string = substring_se(trimmed, brace_pos + 1, cast[i32](str_len(trimmed) - 1));

                    if (str_len(type_name) == 0 or equals_c(prefix, type_name.data)) {
                        StringBuilder.append_c(addr(sb_decl), " = {");

                        mut sb_current2: StringBuilder = StringBuilder.init(str_len(inner) + 16);
                        mut depth2: i32 = 0;
                        mut first_field2: bool = true;
                        mut i3: i32 = 0;
                        val inner_len: i32 = cast[i32](str_len(inner));

                        loop {
                            if i3 >= inner_len {
                                break;
                            }
                            val ch2: char = get_char(inner, i3);
                            if ch2 == '(' {
                                depth2 = depth2 + 1;
                                StringBuilder.append_char(addr(sb_current2), ch2);
                            } elif ch2 == ')' {
                                if depth2 > 0 {
                                    depth2 = depth2 - 1;
                                }
                                StringBuilder.append_char(addr(sb_current2), ch2);
                            } elif ch2 == ',' and depth2 == 0 {
                                mut part2: string = strip(StringBuilder.to_string(addr(sb_current2)));
                                if str_len(part2) > 0 {
                                    val colon_pos2: i32 = find_char_from(part2, ':', cast[usize](0));
                                    if colon_pos2 >= 0 {
                                        val field_name2: string = strip(substring_se(part2, 0, colon_pos2));
                                        val value_expr2: string = strip(substr(part2, colon_pos2 + 1, cast[i32](str_len(part2) - colon_pos2 - 1)));
                                        if str_len(field_name2) > 0 and str_len(value_expr2) > 0 {
                                            if !first_field2 {
                                                StringBuilder.append_c(addr(sb_decl), ", ");
                                            }
                                            StringBuilder.append_c(addr(sb_decl), ".");
                                            StringBuilder.append(addr(sb_decl), field_name2);
                                            StringBuilder.append_c(addr(sb_decl), " = ");
                                            val converted_value2: string = convert_union_member_init(value_expr2);
                                            StringBuilder.append(addr(sb_decl), process_expression(converted_value2));
                                            first_field2 = false;
                                        }
                                    }
                                }
                                StringBuilder.clear(addr(sb_current2));
                            } else {
                                StringBuilder.append_char(addr(sb_current2), ch2);
                            }
                            i3 = i3 + 1;
                        }

                        mut part_last2: string = strip(StringBuilder.to_string(addr(sb_current2)));
                        StringBuilder.destroy(addr(sb_current2));
                        if str_len(part_last2) > 0 {
                            val colon_pos_last2: i32 = find_char_from(part_last2, ':', cast[usize](0));
                            if colon_pos_last2 >= 0 {
                                val field_name_last2: string = strip(substring_se(part_last2, 0, colon_pos_last2));
                                val value_expr_last2: string = strip(substr(part_last2, colon_pos_last2 + 1, cast[i32](str_len(part_last2) - colon_pos_last2 - 1)));
                                if str_len(field_name_last2) > 0 and str_len(value_expr_last2) > 0 {
                                    if !first_field2 {
                                        StringBuilder.append_c(addr(sb_decl), ", ");
                                    }
                                    StringBuilder.append_c(addr(sb_decl), ".");
                                    StringBuilder.append(addr(sb_decl), field_name_last2);
                                    StringBuilder.append_c(addr(sb_decl), " = ");
                                    val converted_value_last2: string = convert_union_member_init(value_expr_last2);
                                    StringBuilder.append(addr(sb_decl), process_expression(converted_value_last2));
                                    first_field2 = false;
                                }
                            }
                        }

                        if first_field2 {
                            StringBuilder.append_c(addr(sb_decl), "}");
                        } else {
                            StringBuilder.append_c(addr(sb_decl), "}");
                        }
                    } else {
                        val processed_init2: string = process_expression(trimmed);
                        StringBuilder.append_c(addr(sb_decl), " = ");
                        StringBuilder.append(addr(sb_decl), processed_init2);
                    }
                } else {
                    val paren_pos: i32 = find_char_from(trimmed, '(', cast[usize](0));
                    if paren_pos > 0 {
                        val call_name_raw: string = strip(substring_se(trimmed, 0, paren_pos));
                        if StringStringMap.contains(addr(g_function_prefixes), call_name_raw) {
                            val full_name: string = StringStringMap.get(addr(g_function_prefixes), call_name_raw);
                            val rest: string = substring_se(trimmed, paren_pos, cast[i32](str_len(trimmed)));
                            trimmed = concat(full_name, rest);
                        }

                        if equals_c(call_name_raw, "cast") {
                            mut has_type_params: bool = false;
                            mut check_pos: i32 = cast[i32](str_len(call_name_raw));
                            loop {
                                if check_pos >= paren_pos {
                                    break;
                                }
                                val ch: char = get_char(trimmed, check_pos);
                                if ch == '[' {
                                    has_type_params = true;
                                    break;
                                }
                                check_pos = check_pos + 1;
                            }

                            if !has_type_params {
                                if str_len(type_name) > 0 and !equals_c(type_name, "auto") and !equals_c(type_name, "cast_inferred_type") {
                                    mut depth: i32 = 1;
                                    mut end_pos: i32 = paren_pos + 1;
                                    mut in_str: bool = false;
                                    mut in_char: bool = false;
                                    loop {
                                        if end_pos >= str_len(trimmed) {
                                            break;
                                        }
                                        val ch: char = get_char(trimmed, end_pos);
                                        if ch == '"' and !in_char {
                                            in_str = !in_str;
                                        } elif ch == '\'' and !in_str {
                                            in_char = !in_char;
                                        } elif !in_str and !in_char {
                                            if ch == '(' {
                                                depth = depth + 1;
                                            } elif ch == ')' {
                                                depth = depth - 1;
                                                if depth == 0 {
                                                    break;
                                                }
                                            }
                                        }
                                        end_pos = end_pos + 1;
                                    }

                                    if end_pos < str_len(trimmed) and get_char(trimmed, end_pos) == ')' {
                                        val args_content: string = strip(substring_se(trimmed, paren_pos + 1, end_pos)); // Extract content between ( and )
                                        mut cast_with_type: string = concat(str("cast["), type_name);
                                        cast_with_type = concat(cast_with_type, str("]("));
                                        cast_with_type = concat(cast_with_type, args_content);
                                        cast_with_type = concat(cast_with_type, str(")"));
                                        trimmed = cast_with_type;
                                    }
                                } elif !has_type_params and str_len(type_name) == 0 {
                                    print(get_short_filename(g_current_source_file));
                                    print(":");
                                    print(i32_to_string(g_current_line));
                                    print(": error: cannot infer cast target type for '");
                                    print(trimmed);
                                    println("', add explicit type to the LHS.");
                                    unsafe {
                                        C.exit(1);
                                    }
                                }
                            }
                        }
                    }

                    val old_expected_type: string = g_expected_return_type;
                    mut expected_type_for_call: string = type_name;
                    if g_current_type_mapping != nil and StringStringMap.contains(g_current_type_mapping, type_name) {
                        expected_type_for_call = StringStringMap.get(g_current_type_mapping, type_name);
                    }
                    g_expected_return_type = expected_type_for_call;
                    val processed_init: string = process_expression(trimmed);
                    g_expected_return_type = old_expected_type;

                    StringBuilder.append_c(addr(sb_decl), " = ");
                    StringBuilder.append(addr(sb_decl), processed_init);
                }
            }
        } else {
            StringBuilder.append_c(addr(sb_decl), " = {0}");
        }
        
        StringBuilder.append_c(addr(sb_decl), ";\n");
        result = StringBuilder.to_string(addr(sb_decl));
        StringBuilder.destroy(addr(sb_decl));
        return result;
    }
    
    if equals_c(node_type, "Assignment") {
        val variable: string = ast.data.assignment.variable;
        val expression: string = ast.data.assignment.expression;
        
        debug_print_raw "\n[DBG-ASSIGN] Assignment node";
        debug_print_raw "\n[DBG-ASSIGN]   raw variable:";
        debug_print_str variable;
        debug_print_raw "\n[DBG-ASSIGN]   raw expression:";
        debug_print_str expression;

        debug_print_raw "\n[DBG-CALL] process_expression from Assignment.variable:";
        debug_print_str variable;
        val processed_var: string = process_expression(variable);
        debug_print_raw "\n[DBG-CALL] process_expression from Assignment.expression:";
        debug_print_str expression;
        val processed_expr: string = process_expression(expression);

        debug_print_raw "\n[DBG-ASSIGN]   processed variable:";
        debug_print_str processed_var;
        debug_print_raw "\n[DBG-ASSIGN]   processed expression:";
        debug_print_str processed_expr;
        
        val pv: string = processed_var;
        debug_print_raw "\n[DBG-ASSIGN]   pv before fix:";
        debug_print_str pv;

        mut sb_fixed_var: StringBuilder = StringBuilder.init(str_len(pv) + 16);
        mut idx_av: i32 = 0;
        val pv_len: i32 = cast[i32](str_len(pv));
        loop {
            if idx_av >= pv_len {
                break;
            }
            val chx: char = get_char(pv, idx_av);

            if chx == '(' and idx_av > 0 {
                val prev_ch: char = get_char(pv, idx_av - 1);
                if (prev_ch >= 'a' and prev_ch <= 'z') or
                   (prev_ch >= 'A' and prev_ch <= 'Z') or
                   (prev_ch >= '0' and prev_ch <= '9') or
                   prev_ch == '_' {

                    mut k2: i32 = idx_av + 1;
                    loop {
                        if k2 >= pv_len {
                            break;
                        }
                        val ws2: char = get_char(pv, k2);
                        if ws2 != ' ' and ws2 != '\t' and ws2 != '\n' {
                            break;
                        }
                        k2 = k2 + 1;
                    }

                    if k2 + 1 < pv_len and
                       get_char(pv, k2) == '-' and
                       get_char(pv, k2 + 1) == '>' {
                        idx_av = k2;
                        continue;
                    }
                }
            }

            StringBuilder.append_char(addr(sb_fixed_var), chx);
            idx_av = idx_av + 1;
        }

        mut fixed_var: string = StringBuilder.to_string(addr(sb_fixed_var));
        StringBuilder.destroy(addr(sb_fixed_var));

        mut out_lhs: string = fixed_var;
        mut out_rhs: string = processed_expr;
        mut ri: i32 = 0;

        loop {
            if ri >= str_len(processed_expr) {
                break;
            }
            val ws0: char = get_char(processed_expr, ri);
            if ws0 != ' ' and ws0 != '\t' and ws0 != '\n' {
                break;
            }
            ri = ri + 1;
        }

        if ri < str_len(processed_expr) and is_ident_start(get_char(processed_expr, ri)) {
            val istart: i32 = ri;
            mut j: i32 = ri + 1;
            loop {
                if j >= str_len(processed_expr) {
                    break;
                }
                val cj: char = get_char(processed_expr, j);
                if !((cj >= 'a' and cj <= 'z') or (cj >= 'A' and cj <= 'Z') or (cj >= '0' and cj <= '9') or cj == '_') {
                    break;
                }
                j = j + 1;
            }

            val enum_ident: string = substring_se(processed_expr, istart, j);

            mut k: i32 = j;
            loop {
                if k >= str_len(processed_expr) {
                    break;
                }
                val ws1: char = get_char(processed_expr, k);
                if ws1 != ' ' and ws1 != '\t' and ws1 != '\n' {
                    break;
                }
                k = k + 1;
            }

            if k < str_len(processed_expr) and get_char(processed_expr, k) == '.' {
                k = k + 1;
                loop {
                    if k >= str_len(processed_expr) {
                        break;
                    }
                    val ws2: char = get_char(processed_expr, k);
                    if ws2 != ' ' and ws2 != '\t' and ws2 != '\n' {
                        break;
                    }
                    k = k + 1;
                }

                if k < str_len(processed_expr) and is_ident_start(get_char(processed_expr, k)) {
                    val mstart: i32 = k;
                    mut m: i32 = k + 1;
                    loop {
                        if m >= str_len(processed_expr) {
                            break;
                        }
                        val cm: char = get_char(processed_expr, m);
                        if !((cm >= 'a' and cm <= 'z') or (cm >= 'A' and cm <= 'Z') or (cm >= '0' and cm <= '9') or cm == '_') {
                            break;
                        }
                        m = m + 1;
                    }

                    val member: string = substring_se(processed_expr, mstart, m);
                    val enum_c_name2: string = canonical_model_c_name(enum_ident);

                    if str_len(enum_c_name2) > 0 {
                        mut rebuilt: string = str("");
                        if ri > 0 {
                            rebuilt = substring_se(processed_expr, 0, ri);
                        }
                        rebuilt = concat(rebuilt, enum_c_name2);
                        rebuilt = concat(rebuilt, str("_"));
                        rebuilt = concat(rebuilt, member);
                        if m < str_len(processed_expr) {
                            val suffix: string = substring_se(processed_expr, m, cast[i32](str_len(processed_expr)));
                            rebuilt = concat(rebuilt, suffix);
                        }
                        out_rhs = rebuilt;
                    }
                }
            }
        }

        if has_prefix(processed_expr, str("->")) {
            val eq_pos: i32 = find_char_from(processed_expr, '=', cast[usize](0));
            if eq_pos > 0 {
