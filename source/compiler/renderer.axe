                                StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(type2)));
                                StringBuilder.append_c(addr(sb), "__T_");
                                StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(type1)));
                            } elif num_type_params == 1 and str_len(type2) > 0 {
                                StringBuilder.append_c(addr(sb), "__T_");
                                StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(type2)));
                            } elif str_len(type1) > 0 {
                                StringBuilder.append_c(addr(sb), "__T_");
                                StringBuilder.append(addr(sb), sanitize_c_identifier(map_axe_type_to_c(type1)));
                            }
                            i = k;
