            if k < expr_len and get_char(expr, k) == '(' {
                mut is_generic_match: bool = StringBoolMap.contains(addr(g_generic_functions), ident);
                if !is_generic_match {
                    mut last_sep: i32 = -1;
                    mut ji: i32 = 0;
                    val ilen: i32 = cast[i32](str_len(ident));
                    loop {
                        if ji + 1 >= ilen { break; }
                        val c1: char = get_char(ident, ji);
                        val c2: char = get_char(ident, ji + 1);
                        if c1 == '_' and c2 == '_' {
                            last_sep = ji;
                        }
                        ji++;
                    }
                    if last_sep > 0 {
                        val suffix: string = substring_se(ident, last_sep + 2, ilen);
                        if StringBoolMap.contains(addr(g_generic_functions), suffix) {
                            is_generic_match = true;
                        }
                    }

                     if !is_generic_match {
                         mut is_method_part: bool = false;
                         mut back_scan: i32 = ident_start - 1;
                         loop {
                             if back_scan < 0 { break; }
                             val bc: char = get_char(expr, back_scan);
                             if bc != ' ' and bc != '\t' and bc != '\n' {
                                 if bc == '.' or (bc == '>' and back_scan > 0 and get_char(expr, back_scan - 1) == '-') {
                                     is_method_part = true;
                                 }
                                 break;
                             }
                             back_scan--;
