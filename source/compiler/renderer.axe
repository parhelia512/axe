                    }
                    
                    if m < expr_len and get_char(expr, m) == '(' {
                        if StringStringMap.contains(addr(g_model_names), ident) or is_symbol_imported(ident) {
                            mut sb_check: StringBuilder = StringBuilder.init(str_len(ident) + str_len(method_name) + 2);
