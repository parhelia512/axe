                if body != nil {
                    mut i_defer: i32 = 0;
                    loop {
                        if i_defer >= len(deref(body)) {
                            break;
                        }
                        val defer_child_code: string = generate_c(addr(body.data[i_defer]));
                        StringBuilder.append(addr(sb_for), defer_child_code);
                        i_defer++;
                    }
                }
                defer_idx++;
            }
        }

        StringBuilder.append_c(addr(sb_for), "}\n");
        
        if loop_var_added {
            StringStringMap.pop(addr(g_var_types), loop_var_name);
        }

        return StringBuilder.to_string(addr(sb_for));
    }
    
    if equals_c(node_type, "ParallelFor") {
        val init_raw: string = ast.data.parallel_for.initialization;
        val cond_raw: string = ast.data.parallel_for.condition;
        val incr_raw: string = ast.data.parallel_for.increment;

        mut init: string = strip(init_raw);
        val cond: string = strip(cond_raw);
        mut incr: string = strip(incr_raw);
        
        if has_suffix(incr, str(";")) {
            incr = strip(substring_se(incr, 0, cast[i32](str_len(incr) - 1)));
        }

        if has_prefix(init, str("mut ")) {
            init = strip(substr(init, 4, str_len(init) - 4));
        }

        mut loop_var_name: string = str("");
        mut loop_var_added: bool = false;

        val eq_pos: i32 = find_char_from(init, '=', cast[usize](0));
        if eq_pos > 0 {
            mut v_end: i32 = eq_pos - 1;
            loop {
                if v_end < 0 { break; }
                val ch: char = get_char(init, v_end);
                if ch != ' ' and ch != '\t' { break; }
                v_end = v_end - 1;
            }
            
            if v_end >= 0 {
                mut v_start: i32 = v_end;
                loop {
                    if v_start < 0 { break; }
