use std.io;

def fib(n: i32): i32 {
    if n <= 1 {
        return n;
    }

    mut a: i32 = 0;
    mut b: i32 = 1;
    mut i: i32 = 2;

    loop {
        if i > n {
            break;
        }

        mut temp: i32 = a + b;
        a = b;
        b = temp;

        i = i + 1;
    }

    return b;
}

def main() {
    mut i: i32 = 0;
    loop {
        if i > 20 {
            break;
        }

        print "fib(";
        print i;
        print ") = ";
        println(fib(i));

        i = i + 1;
    }
}
