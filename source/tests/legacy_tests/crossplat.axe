use std.io;

def cross_platform_func() {
    platform windows {
        parallel for mut i = 0; i < 10; i++ {
            println "windows:";
            println i;
        }
    }
    platform posix {
        parallel for mut i = 0; i < 10; i++ {
            println "posix:";
            println i;
        }
    }
}

def main() {
    cross_platform_func();
}